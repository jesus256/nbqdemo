                          <xs:documentation>If childrens pension required then the details are specified here.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element maxOccurs="unbounded" name="Child">
                              <xs:complexType>
                                <xs:all>
                                  <xs:element name="ChildName" type="xs:string"/>
                                  <xs:element name="ChildPercentage" type="xs:double">
                                    <xs:annotation>
                                      <xs:documentation>Childs percentage of the members pension.</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                  <xs:element name="Age">
                                    <xs:annotation>
                                      <xs:documentation>Age of child in years and months</xs:documentation>
                                    </xs:annotation>
                                    <xs:complexType>
                                      <xs:attribute name="years" type="xs:int" use="required"/>
                                      <xs:attribute name="months" type="xs:int" use="optional"/>
                                    </xs:complexType>
                                  </xs:element>
                                  <xs:element name="StartOnFirstDeath" type="xs:boolean">
                                    <xs:annotation>
                                      <xs:documentation>Whether or not the child's payment starts on the dearth  of first life or both lives</xs:documentation>
                                    </xs:annotation>
                                  </xs:element>
                                </xs:all>
                              </xs:complexType>
                            </xs:element>
                          </xs:sequence>
                          <xs:attribute name="startOnFirstDeath" type="xs:boolean" use="required"/>
                        </xs:complexType>
                      </xs:element>
                      <xs:element name="AdvanceIndicator" type="xs:boolean">
                        <xs:annotation>
                          <xs:documentation>Whether payments start when the annuity is purchased or at the end of the first frequential payment period.</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element name="PaymentFrequency" type="enums:EnumFrequency">
                        <xs:annotation>
                          <xs:documentation>Frequency of pension payment.</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element name="GuaranteePeriod">
                        <xs:annotation>
                          <xs:documentation>Period during which the members pension from inception will be paid out even if the member dies during this period. </xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:attribute name="years" type="xs:int" use="required"/>
                          <xs:attribute name="months" type="xs:int" use="optional"/>
                        </xs:complexType>
                      </xs:element>
                      <xs:element name="Escalation">
                        <xs:complexType>
                          <xs:choice>
                            <xs:element name="EscalationRate" type="xs:double">
                              <xs:annotation>
                                <xs:documentation>The rate at which your pension will increase each year.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element name="CPIEscalationIdentifier" type="xs:string">
                              <xs:annotation>
                                <xs:documentation>This identifier will match a CPI escalation rule in the configuration</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                          </xs:choice>
                          <xs:attribute name="escalationStartMonth" type="enums:EnumMonthOfYear" use="optional"/>
                        </xs:complexType>
                      </xs:element>
                      <xs:element name="CommissionOption" type="xs:string">
                        <xs:annotation>
                          <xs:documentation>The rate of commission paid to the sales broker.</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element minOccurs="0" name="ExpertUserOverrides">
                        <xs:annotation>
                          <xs:documentation>These values will only be used if the expert user service is invoked.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:all>
                            <xs:element minOccurs="0" name="MortTableOverride" type="xs:string">
                              <xs:annotation>
                                <xs:documentation>Overrides the mortality rate table used by the annuity engine.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element minOccurs="0" name="InterestRateOverride" type="xs:double">
                              <xs:annotation>
                                <xs:documentation>Overrides the interest rate used by the annuity engine 9i.e. the yield value).</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element minOccurs="0" name="ExpenseRateOverride" type="xs:double">
                              <xs:annotation>
                                <xs:documentation>Overrides the expense rate used by the annuity engine.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element minOccurs="0" name="PolicyFeeOverride" type="xs:double">
                              <xs:annotation>
                                <xs:documentation>Amount to be used for the monthly policy fee.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                          </xs:all>
                        </xs:complexType>
                      </xs:element>
                      <xs:element minOccurs="0" name="EnhancedDetails">
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element name="Client">
                              <xs:complexType>
                                <xs:attribute name="MortalityRateUplift" type="xs:double"/>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="Spouse">
                              <xs:complexType>
                                <xs:attribute name="MortalityRateUplift" type="xs:double"/>
                              </xs:complexType>
                            </xs:element>
                            <xs:element minOccurs="0" name="Height">
                              <xs:complexType>
                                <xs:attribute name="Feet" type="xs:int"/>
                                <xs:attribute name="Inches" type="xs:int"/>
                                <xs:attribute name="Centimetres" type="xs:int"/>
                                <xs:attribute name="SpouseFeet" type="xs:int"/>
                                <xs:attribute name="SpouseInches" type="xs:int"/>
                                <xs:attribute name="SpouseCentimetres" type="xs:int"/>
                              </xs:complexType>
                            </xs:element>
                            <xs:element minOccurs="0" name="Weight">
                              <xs:complexType>
                                <xs:attribute name="Stone" type="xs:int"/>
                                <xs:attribute name="Pounds" type="xs:int"/>
                                <xs:attribute name="Kilos" type="xs:int"/>
                                <xs:attribute name="SpouseStone" type="xs:int"/>
                                <xs:attribute name="SpousePounds" type="xs:int"/>
                                <xs:attribute name="SpouseKilos" type="xs:int"/>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="BMI">
                              <xs:complexType>
                                <xs:attribute name="Client" type="xs:boolean"/>
                                <xs:attribute name="Spouse" type="xs:boolean"/>
                              </xs:complexType>
                            </xs:element>
                            <xs:element minOccurs="0" name="Questions">
                              <xs:complexType>
                                <xs:sequence maxOccurs="unbounded">
                                  <xs:element maxOccurs="unbounded" minOccurs="0" name="Question">
                                    <xs:complexType>
                                      <xs:attribute name="QuestionSeq" type="xs:int" use="required"/>
                                      <xs:attribute name="QuestionNo" type="xs:int" use="required"/>
                                      <xs:attribute name="QuestionType" type="xs:int" use="required"/>
                                      <xs:attribute name="QuestionText" type="xs:string" use="required"/>
                                      <xs:attribute name="ClientAns" type="xs:boolean" use="optional"/>
                                      <xs:attribute name="SpouseAns" type="xs:boolean"/>
                                      <xs:attribute name="Message" type="xs:string"/>
                                    </xs:complexType>
                                  </xs:element>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                          </xs:sequence>
                          <xs:attribute name="EntitledToEnhancedAnnuity" type="xs:boolean"/>
                        </xs:complexType>
                      </xs:element>
                    </xs:all>
                  </xs:complexType>
                </xs:element>
                <xs:element name="PensionProjection">
                  <xs:annotation>
                    <xs:documentation>Supply this information if the annuity is to be used for a pension projections quote. Note: the system will determine if the quote needs to switch to a New Business quote and make the adjustments itself. </xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:all>
                      <xs:element name="RetirementDate" type="xs:dateTime">
                        <xs:annotation>
                          <xs:documentation>Date of member retirement.</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element minOccurs="0" name="SpouseDetails">
                        <xs:annotation>
                          <xs:documentation>If spouse pension required then the details are specified here. Any details which are not known will be defaulted</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:all>
                            <xs:element name="SpousesPercentage" type="xs:double"/>
                            <xs:element minOccurs="0" name="Overlap" type="xs:boolean">
                              <xs:annotation>
                                <xs:documentation>If not supplied this field will be defaulted if there is a spouse requirement on this quote.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element minOccurs="0" name="Sex" type="enums:EnumGender">
                              <xs:annotation>
                                <xs:documentation>If not supplied this field will be defaulted if there is a spouse requirement on this quote.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element minOccurs="0" name="SpouseAgeOrDOB" type="AgeOrDOB">
                              <xs:annotation>
                                <xs:documentation>If not supplied this field will be defaulted if there is a spouse requirement on this quote.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element minOccurs="0" name="SpouseEscalationRate" type="xs:double">
                              <xs:annotation>
                                <xs:documentation>The rate at which your pension will increase each year for the spouse. If not known the main escalation rate will be used. </xs:documentation>
                              </xs:annotation>
                            </xs:element>
                          </xs:all>
                        </xs:complexType>
                      </xs:element>
                      <xs:element minOccurs="0" name="AdvanceIndicator" type="xs:boolean">
                        <xs:annotation>
                          <xs:documentation>Whether payments start when the annuity is purchased or at the end of the first frequential payment period. If not supplied this value will be defaulted</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element minOccurs="0" name="PaymentFrequency" type="enums:EnumFrequency">
                        <xs:annotation>
                          <xs:documentation>Frequency of pension payment. If not supplied this value will be defaulted.</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element minOccurs="0" name="GuaranteePeriod">
                        <xs:annotation>
                          <xs:documentation>Period during which the members pension from inception will be paid out even if the member dies during this period.  If not supplied this value will be defaulted.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:attribute name="years" type="xs:int" use="required"/>
                          <xs:attribute name="months" type="xs:int" use="optional"/>
                        </xs:complexType>
                      </xs:element>
                      <xs:element minOccurs="0" name="EscalationRate" type="xs:double">
                        <xs:annotation>
                          <xs:documentation>The rate at which your pension will increase each year. If not supplied this value will be defaulted.</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element minOccurs="0" name="CommissionOption" type="xs:string">
                        <xs:annotation>
                          <xs:documentation>The rate of commission paid to the sales broker. If not supplied this value will be defaulted. Note; this value is only applicable to PP quotes which have been switched to NB annuity quotes. </xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element minOccurs="0" name="CPI" type="xs:double">
                        <xs:annotation>
                          <xs:documentation>An optionaly supplied value for CPI. Note; this value is only applicable to PP quotes which have been switched to NB annuity quotes. </xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element name="ForProduct" type="enums:EnumProductCategory">
                        <xs:annotation>
                          <xs:documentation>This lets the quote system determine the source of pension. Note; this value is only applicable to PP quotes which have been switched to NB annuity quotes. </xs:documentation>
                        </xs:annotation>
                      </xs:element>
                    </xs:all>
                  </xs:complexType>
                </xs:element>
              </xs:choice>
            </xs:complexType>
          </xs:element>
          <xs:element name="QuoteDetails">
            <xs:complexType>
              <xs:choice>
                <xs:element name="NewBusinessQuote">
                  <xs:annotation>
                    <xs:documentation>Only useable in a new business annuity quote scenario.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:choice>
                      <xs:element name="Fund">
                        <xs:annotation>
                          <xs:documentation>If supplied the output will contain pension and reduced pension amounts.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:attribute name="purchaseMoney" type="xs:double" use="required"/>
                          <xs:attribute name="taxFreeCashRequired" type="xs:double" use="optional"/>
                        </xs:complexType>
                      </xs:element>
                      <xs:element name="PensionRequired">
                        <xs:annotation>
                          <xs:documentation>If supplied the output will contain the Purchase Money required to achieve this pension.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:attribute name="annualPension" type="xs:double" use="required"/>
                          <xs:attribute name="taxFreeCashRequired" type="xs:double" use="optional"/>
                        </xs:complexType>
                      </xs:element>
                    </xs:choice>
                  </xs:complexType>
                </xs:element>
                <xs:element name="PensionBreakdown">
                  <xs:annotation>
                    <xs:documentation>Only useable in a pension projection quote scenario. Includes pension breakdown information in the output.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:all>
                      <xs:element name="PurchaseMoney" type="xs:double">
                        <xs:annotation>
                          <xs:documentation>Projected fund available at retirement age</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element name="InflationRate" type="xs:double">
                        <xs:annotation>
                          <xs:documentation>Inflation rate is used to convert between future and todays terms.</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element minOccurs="0" name="SalaryInfo" type="SalaryInformation"/>
                      <xs:element minOccurs="0" name="ExecPensionInputs" type="ExecutivePensInputs">
                        <xs:annotation>
                          <xs:documentation>Only populated for executive pensions</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                    </xs:all>
                  </xs:complexType>
                </xs:element>
                <xs:element name="TargetFund">
                  <xs:annotation>
                    <xs:documentation>Only useable in a pension projection quote scenario. Includes the fund required to achieve the target on the output. </xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:all>
                      <xs:element name="PensionTargetType" type="enums:EnumPensionTargetType">
                        <xs:annotation>
                          <xs:documentation>Type of target requested by the client. </xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element minOccurs="0" name="TargetValue" type="xs:double">
                        <xs:annotation>
                          <xs:documentation>Value of the Pension Required (this value is related to the pension target type. This value is not required when the type is Max Tax Free Lump Sum)</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element minOccurs="0" name="SalaryInfo" type="SalaryInformation"/>
                    </xs:all>
                  </xs:complexType>
                </xs:element>
                <xs:element name="MaxPremiumAllowed">
                  <xs:annotation>
                    <xs:documentation>Only useable in a pension projection quote scenario. Includes the maximum executive pension premium amount allowed</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:all>
                      <xs:element name="SalaryInfo" type="SalaryInformation"/>
                      <xs:element name="ExecPensionInputs" type="ExecutivePensInputs"/>
                      <xs:element name="RetainedBenefits">
                        <xs:annotation>
                          <xs:documentation>Informarion about other pension arrangements outside this one</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:attribute name="assetsPlusRetainedBenefits" type="xs:double" use="required"/>
                          <xs:attribute name="includeRetainedBenefits" type="xs:boolean" use="required"/>
                          <xs:attribute name="unknownRetainedBenefitsFlag" type="xs:boolean" use="required"/>
                        </xs:complexType>
                      </xs:element>
                    </xs:all>
                  </xs:complexType>
                </xs:element>
              </xs:choice>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="QuoteVariants">
            <xs:annotation>
              <xs:documentation>For a MultiQuote this node lists the items to be varied. </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element maxOccurs="unbounded" name="Variant" type="enums:EnumAnnuityMultiQuoteVariants"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="LinkedToPolicyNr" type="xs:string">
            <xs:annotation>
              <xs:documentation>Policy number of the policy this quote was run for. This is for information purposes only. </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="SellerInfo">
            <xs:complexType>
              <xs:attribute name="AgencyExtraCommission" type="xs:double"/>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="SourceOfPension" type="enums:EnumSourceOfPension">
            <xs:annotation>
              <xs:documentation>Can be used to specify the source of pension funding which can have a bearing on whether to use gender neutral rates or not. This setting is only used for direct NB quotes. For PP quotes which get converted to NB quote the ForProduct node is used to derive this field.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element default="false" minOccurs="0" name="UseReducedRateForReducedPension" type="xs:boolean"/>
        </xs:all>
      </xs:complexType>
      <xs:complexType name="SalaryInformation">
        <xs:attribute name="annualSalary" type="xs:double" use="required"/>
        <xs:attribute name="salaryEscalationRate" type="xs:double" use="required"/>
      </xs:complexType>
      <xs:complexType name="AgeOrDOB">
        <xs:choice>
          <xs:element name="DOB" type="xs:dateTime"/>
          <xs:element name="Age">
            <xs:complexType>
              <xs:attribute name="years" type="xs:int" use="required"/>
              <xs:attribute name="months" type="xs:int" use="optional"/>
            </xs:complexType>
          </xs:element>
        </xs:choice>
      </xs:complexType>
      <xs:complexType name="ExecutivePensInputs">
        <xs:sequence>
          <xs:element name="DateOfJoiningService" type="xs:dateTime">
            <xs:annotation>
              <xs:documentation>Used in the calculation of service term.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="DateOfLeavingService" type="xs:dateTime">
            <xs:annotation>
              <xs:documentation>Used in the calculation of service term.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="IsTwentyPercentDirector" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>Indicates if the member a 20% director</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
    </xs:schema>
    <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://projectionsref.framework.domain.projections.bil.ie" xmlns:enums="http://enums.common.framework.domain.projections.bil.ie" xmlns:generaltypes="http://generaltypes.common.framework.domain.projections.bil.ie" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://projectionsref.framework.domain.projections.bil.ie">
      <xs:import namespace="http://enums.common.framework.domain.projections.bil.ie"/>
      <xs:import namespace="http://generaltypes.common.framework.domain.projections.bil.ie"/>
      <xs:element name="ProjectionsRefRoot">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="ProjectionsRef" type="ProjectionsRef"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:complexType name="ProjectionsRef">
        <xs:annotation>
          <xs:documentation>This data set contains general reference information used by the projections system.  This data applies to all products or all products of a particular type.</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="DefaultQuotationBasis">
            <xs:annotation>
              <xs:documentation>Contains a list of the default growth cap and indexation rates.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element maxOccurs="unbounded" name="BasisRow" type="generaltypes:BasisRowDef"/>
                <xs:element maxOccurs="unbounded" minOccurs="0" name="ProductSpecific">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" name="BasisRow" type="generaltypes:BasisRowDef"/>
                    </xs:sequence>
                    <xs:attribute name="productCat" type="enums:EnumProductCategory" use="required"/>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element maxOccurs="unbounded" name="AdminSysDependant">
            <xs:annotation>
              <xs:documentation>This section contains settings which are specific to particular  administrations. </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:all>
                <xs:element name="OrderOfEvents">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" name="EventRows" type="EventRow">
                        <xs:annotation>
                          <xs:documentation>The order is determined by the order of this list.</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:all>
              <xs:attribute name="adminSystem" type="enums:EnumSourceOfData" use="required"/>
            </xs:complexType>
          </xs:element>
          <xs:element name="Targeting">
            <xs:annotation>
              <xs:documentation>Settings relating to targeting tolerence which apply to all quotations.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:all>
                <xs:element name="ProjectionFundTolerance" type="xs:double"/>
                <xs:element name="RIYTolerance" type="xs:double"/>
                <xs:element name="ParameterGapMonitorTolerance" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>This tolerance meassures the percentage difference between the target parameter bounds. When the percentage difference between these 2 parameters falls below this tolerance then targeting will finish. This measure is used to ensure that targeting can complete it's work even if the target fund is unatainable due to premium size banding. </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="MaxTargetingIterations" type="xs:int">
                  <xs:annotation>
                    <xs:documentation>This is a failsafe mechanism only.  It is the maximum numer of attempts that the engine will use to try to calculate the target fund. </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="TargetParamSettings">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" name="TargetParamRow">
                        <xs:complexType>
                          <xs:attribute name="targetParameter" type="enums:EnumProjectionEvent" use="required"/>
                          <xs:attribute name="lowerBandSetting" type="xs:double" use="required"/>
                          <xs:attribute name="upperBandSetting" type="xs:double" use="required"/>
                          <xs:attribute name="hasIncrementalEffect" type="xs:boolean" use="required"/>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element name="MinimumEmployerContPercent" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>This is used in conjunction with the targeting quote scenario setting which collects the members desire to supply a certain percentage of their sal of the calculated target.  This is the minimum employer percentage that is allowed in this calc. </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="NonLinkedTargetingTolerance" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>Used for non-linked quotes only. If there is a non-linked target this value is used to decide when to stop targeting. </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="NonLinkedTargetingCoefficient" type="xs:double"/>
              </xs:all>
            </xs:complexType>
          </xs:element>
          <xs:element name="TaxRate">
            <xs:annotation>
              <xs:documentation>The tax rate applied for Life policies is 3% above the standard rate of income tax (currently 20%)</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:attribute name="exitTaxRate" type="xs:double" use="required"/>
              <xs:attribute name="exitTaxRateEffectiveDate" type="xs:dateTime" use="required"/>
              <xs:attribute name="defaultPersonalTaxRate" type="xs:double" use="required"/>
              <xs:attribute name="defaultCorpTaxRate" type="xs:double" use="required">
                <xs:annotation>
                  <xs:documentation>Used to net the growth rate used in net type products. </xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="corpExitTaxRate" type="xs:double" use="required">
                <xs:annotation>
                  <xs:documentation>Required to be held from some ABS docs</xs:documentation>
                </xs:annotation>
              </xs:attribute>
            </xs:complexType>
          </xs:element>
          <xs:element name="AgencyInformation">
            <xs:annotation>
              <xs:documentation>Contains the list of all Agents on that system that have Disclosure Uplift Factors that are non-standard</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:all>
                <xs:element name="SpecifiedAgencyInfo">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" name="AgentRow">
                        <xs:annotation>
                          <xs:documentation>Contains the set of DUFs for  particular Agents that DUFs that differe from the defaults</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:sequence>
