                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="DefaultSellerInfo">
                  <xs:annotation>
                    <xs:documentation>This node contains the default seller information for each company.  It is up to the client constructing the Policy Data to use some or all of this information as required. </xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" name="ForCompany">
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element name="BrokerClass" type="xs:string">
                              <xs:annotation>
                                <xs:documentation>Default Broker Class</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element name="InitialUplift" type="xs:double">
                              <xs:annotation>
                                <xs:documentation>Default Initial Commission Uplift</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element name="RenewalUplift" type="xs:double">
                              <xs:annotation>
                                <xs:documentation>Default Renewal Commission Uplift</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                          </xs:sequence>
                          <xs:attribute name="companyIndicator" type="xs:int"/>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element name="MinInitialCommUplift" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>This is the minimum initial commission uplift rate. If the Initial Commisison rate is not supplied or is less than this value, then this value will be used as the Initial Commission Rate</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="GeneralCommUplifts">
                  <xs:annotation>
                    <xs:documentation>Information used to adjust tracker commission give up. </xs:documentation>
                  </xs:annotation>
