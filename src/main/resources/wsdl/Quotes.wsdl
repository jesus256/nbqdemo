                                <xs:sequence>
                                  <xs:element maxOccurs="unbounded" minOccurs="0" name="PremiumDetailChanges">
                                    <xs:annotation>
                                      <xs:documentation>Chanrges to the amount, frequency and indexation</xs:documentation>
                                    </xs:annotation>
                                    <xs:complexType>
                                      <xs:sequence>
                                        <xs:element name="NewPremDetails">
                                          <xs:complexType>
                                            <xs:attribute name="totalPremium" type="xs:double" use="required">
                                              <xs:annotation>
                                                <xs:documentation>Total regular premium amount being paid to the policy each period.  If there are employer and an  employee contributions, this field will record the addition of both amounts.</xs:documentation>
                                              </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="employerPremium" type="xs:double" use="optional">
                                              <xs:annotation>
                                                <xs:documentation>Amount of the regular premium being paid by the employee. This is only applicable to pension products. 
            </xs:documentation>
                                              </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="avcPremium" type="xs:double" use="optional">
                                              <xs:annotation>
                                                <xs:documentation>Amount of AVC premium being paid into the policy each period. </xs:documentation>
                                              </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="paymentFrequency" type="enums:EnumFrequency" use="required"/>
                                            <xs:attribute name="paymentMethod" type="enums:EnumPaymentMode" use="required"/>
                                            <xs:attribute name="extraPremium" type="xs:double" use="optional">
                                              <xs:annotation>
                                                <xs:documentation>Amount of premium rating being paid into the policy each period.  This is a risk benefit rating method used   on      some products. </xs:documentation>
                                              </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="overridePercentage" type="xs:double" use="optional">
                                              <xs:annotation>
                                                <xs:documentation>Allocation rate override.  If the policy has an increaded allocation due to a reduction in commission, this   field   will    contain the current increaded allocation rate. </xs:documentation>
                                              </xs:annotation>
                                            </xs:attribute>
                                            <xs:attribute name="reducedAllocApplies" type="xs:boolean" use="optional"/>
                                            <xs:attribute name="indexationIndicator" type="xs:boolean" use="optional">
                                              <xs:annotation>
                                                <xs:documentation>Use this value to change the indexation status of the policy. </xs:documentation>
                                              </xs:annotation>
                                            </xs:attribute>
                                          </xs:complexType>
                                        </xs:element>
                                      </xs:sequence>
                                      <xs:attribute name="dateOfChange" type="xs:dateTime" use="required"/>
                                      <xs:attribute default="false" name="target" type="xs:boolean" use="optional"/>
                                    </xs:complexType>
                                  </xs:element>
                                  <xs:element minOccurs="0" name="PremiumDirectionChange">
                                    <xs:annotation>
                                      <xs:documentation>New direction of regular premiums being added to the policy.</xs:documentation>
                                    </xs:annotation>
                                    <xs:complexType>
                                      <xs:sequence>
                                        <xs:element maxOccurs="unbounded" name="FundRow">
                                          <xs:complexType>
                                            <xs:attribute name="fundId" type="xs:string" use="required"/>
                                            <xs:attribute name="percentage" type="xs:double" use="required">
                                              <xs:annotation>
                                                <xs:documentation>Percentage of the premium being directed to this fund on allocation.</xs:documentation>
                                              </xs:annotation>
                                            </xs:attribute>
                                          </xs:complexType>
                                        </xs:element>
                                      </xs:sequence>
                                      <xs:attribute name="employerPercentage" type="xs:double" use="required"/>
                                    </xs:complexType>
                                  </xs:element>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                            <xs:element maxOccurs="unbounded" minOccurs="0" name="PremiumStatus">
                              <xs:annotation>
                                <xs:documentation>If this node is supplied the policy data will be changed from the outset from active to paidup or from paidup to active. </xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                <xs:attribute default="true" name="benefitsContinuing" type="xs:boolean"/>
                                <xs:attribute name="dateOfChange" type="xs:dateTime" use="required"/>
                                <xs:attribute name="premiumStatus" type="enums:EnumPremiumStatus" use="required"/>
                                <xs:attribute name="includeOutstandingPremiums" type="xs:boolean" use="optional">
                                  <xs:annotation>
                                    <xs:documentation>Set this flag when the premium status is changed to Premium Paying (from any other status).  This will indicate whether or not the client wishes to pay all arrear premiums. </xs:documentation>
                                  </xs:annotation>
                                </xs:attribute>
                              </xs:complexType>
                            </xs:element>
                            <xs:element maxOccurs="unbounded" minOccurs="0" name="SPInjection">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="SPDev">
                                    <xs:annotation>
                                      <xs:documentation>SP Injuection details</xs:documentation>
                                    </xs:annotation>
