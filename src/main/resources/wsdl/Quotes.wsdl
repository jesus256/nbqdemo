                                            <xs:attribute name="fundId" type="xs:string" use="required"/>
                                            <xs:attribute name="percentage" type="xs:double" use="required">
                                              <xs:annotation>
                                                <xs:documentation>Percentage of the premium being directed to this fund on allocation.</xs:documentation>
                                              </xs:annotation>
                                            </xs:attribute>
                                          </xs:complexType>
                                        </xs:element>
                                      </xs:sequence>
                                      <xs:attribute name="amount" type="xs:double" use="required"/>
                                      <xs:attribute name="investmentOverridePercent" type="xs:double" use="optional">
                                        <xs:annotation>
                                          <xs:documentation>Allocation rate this single premium was allocated to the policy with. </xs:documentation>
                                        </xs:annotation>
                                      </xs:attribute>
                                      <xs:attribute name="agencyNumber" type="xs:string" use="optional">
                                        <xs:annotation>
                                          <xs:documentation>Agency Number of the ageny who brough this new business to the policy if different to the Agency who sold the policy. </xs:documentation>
                                        </xs:annotation>
                                      </xs:attribute>
                                      <xs:attribute name="contributionType" type="enums:EnumContType" use="required">
