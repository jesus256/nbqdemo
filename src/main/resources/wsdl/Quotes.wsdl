                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" name="CommGiveupUplifts">
                        <xs:complexType>
                          <xs:attribute name="agentType" type="enums:EnumSellerChannel" use="required"/>
                          <xs:attribute name="uplift" type="xs:double" use="required"/>
                        </xs:complexType>
                      </xs:element>
                      <xs:element maxOccurs="unbounded" name="FundRelCommGiveupUplifts">
                        <xs:complexType>
                          <xs:attribute name="agentType" type="enums:EnumSellerChannel" use="required"/>
                          <xs:attribute name="uplift" type="xs:double" use="required"/>
                        </xs:complexType>
                      </xs:element>
                      <xs:element maxOccurs="unbounded" name="OptFixedRetAllocUplifts">
