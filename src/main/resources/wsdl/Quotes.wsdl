                          <xs:attribute name="defaultInitialDUF" type="xs:double" use="required"/>
                          <xs:attribute name="defaultRenewalDUF" type="xs:double" use="required"/>
                          <xs:attribute name="defaultSinglePremDUF" type="xs:double" use="required"/>
