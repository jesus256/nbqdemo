                    <xs:attribute name="riskRating" type="xs:int"/>
                    <xs:attribute name="riskTerm" type="xs:int"/>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="riskScore" type="xs:int"/>
                <xs:element minOccurs="0" name="riskTerm" type="xs:int"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="InvestmentType">
