          <xs:element maxOccurs="3" name="Scheme">
            <xs:annotation>
              <xs:documentation>Details of up to 3 schemes will be captured on the UI</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="SchemeName" type="xs:string"/>
