    <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://quotescenario.framework.domain.projections.bil.ie" xmlns:enums="http://enums.common.framework.domain.projections.bil.ie" xmlns:generaltypes="http://generaltypes.common.framework.domain.projections.bil.ie" xmlns:inputvalues="http://inputvalues.framework.domain.projections.bil.ie" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://quotescenario.framework.domain.projections.bil.ie">
      <xs:import namespace="http://enums.common.framework.domain.projections.bil.ie"/>
