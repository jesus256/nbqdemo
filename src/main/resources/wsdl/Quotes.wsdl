                <xs:element name="RegularPremiumSavings" type="xs:decimal"/>
                <xs:element name="Pension"/>
              </xs:choice>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="Recommendations">
            <xs:complexType>
              <xs:sequence>
