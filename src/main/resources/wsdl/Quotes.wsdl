                                <xs:attribute name="policyStatus" type="enums:EnumPremiumStatus" use="optional"/>
                                <xs:attribute name="amount" type="generaltypes:FormattedDouble" use="optional"/>
                                <xs:attribute name="frequency" type="enums:EnumFrequency" use="optional"/>
                              </xs:complexType>
                            </xs:element>
                          </xs:sequence>
                        </xs:complexType>
                      </xs:element>
                      <xs:element name="GovernmentLevyFactor" type="xs:double">
                        <xs:annotation>
                          <xs:documentation>This factor is used to display any premium value inclusive of government levy</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element name="GovernmentLevyGross" type="xs:boolean">
                        <xs:annotation>
                          <xs:documentation>Is the government levy gross of premium?</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element maxOccurs="unbounded" minOccurs="0" name="RPDirectionEvolution">
                        <xs:annotation>
                          <xs:documentation>Contains the fund names for the fund split or RP direction change splits</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element maxOccurs="unbounded" name="FundRow">
                              <xs:complexType>
                                <xs:attribute name="fundName" type="xs:string" use="required"/>
                                <xs:attribute name="percentage" type="xs:double" use="required">
                                  <xs:annotation>
                                    <xs:documentation>Percentage of the premium being directed to this fund on allocation.</xs:documentation>
                                  </xs:annotation>
                                </xs:attribute>
                                <xs:attribute name="fundType" type="enums:EnumFundType" use="required"/>
                                <xs:attribute name="extFundMngrName" type="enums:EnumFundManager" use="optional">
                                  <xs:annotation>
                                    <xs:documentation>If this is an external fund then this is the name of the fund manager</xs:documentation>
                                  </xs:annotation>
                                </xs:attribute>
                              </xs:complexType>
                            </xs:element>
                          </xs:sequence>
                          <xs:attribute name="effectiveDate" type="xs:date" use="required"/>
                        </xs:complexType>
                      </xs:element>
                      <xs:element name="NumberOfIndexations" type="xs:int">
                        <xs:annotation>
                          <xs:documentation>The number of indexations that occurred during the quote</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="PolicyFeeDetails">
                  <xs:annotation>
                    <xs:documentation>Policy Fee used in the quotation and the indexation rate used to increase the policy fee each year. </xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:attribute name="fee" type="xs:double"/>
                    <xs:attribute name="indexationRate" type="xs:double"/>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="EarlyEncashmentChargeAmt" type="generaltypes:FormattedDouble">
                  <xs:annotation>
                    <xs:documentation>This is the surrender penalty amount - appeares on "Investment.dot" docs</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="HasGtdEvrGreenFund" type="xs:boolean">
                  <xs:annotation>
                    <xs:documentation>For use by correspondence</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="SurrPenApplies" type="xs:boolean"/>
                <xs:element name="IncomeFee" type="generaltypes:FormattedDouble">
                  <xs:annotation>
                    <xs:documentation>This is the withdrawal fee.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="TerminalBonusPaid" type="xs:boolean">
                  <xs:annotation>
                    <xs:documentation>For use by correspondence</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="TargetingInfo">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element minOccurs="0" name="TargetContType" type="enums:EnumContType">
                        <xs:annotation>
                          <xs:documentation>This field will be set if one of the contribution types was targeted as part of the quote. </xs:documentation>
                        </xs:annotation>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="AlterationsConstituteATopup" type="xs:boolean">
                  <xs:annotation>
                    <xs:documentation>This value will be set for EBQ cases. It will be true if any alterations to the policy constitute a topup, i.e. any increase in regular premium, any number of sp's added to the policy or indexation has been added where it previously wasn't on the policy. </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="TaxRateUsed" type="generaltypes:FormattedDouble">
                  <xs:annotation>
                    <xs:documentation>If tax is applicable on this policy (either exit tax or income tax) then this is the total rate used in the calculation . </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="TaxRateEffectiveDate" type="xs:string">
                  <xs:annotation>
                    <xs:documentation>The date of effective tax rate (eg January 2011)</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="PensionBoardApprovalNumber" type="xs:string">
                  <xs:annotation>
                    <xs:documentation>The pension board approval number. Used mainly in PRSA products.</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:all>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="ProductInformation">
            <xs:annotation>
              <xs:documentation>Information extracted from the product definition for easy use on reports and UI screens. </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element minOccurs="0" name="PremiumAllocations">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" minOccurs="0" name="RegularPremium">
                        <xs:complexType>
                          <xs:attribute name="upToPremium" type="xs:string"/>
                          <xs:attribute name="charge"/>
                        </xs:complexType>
                      </xs:element>
                      <xs:element maxOccurs="unbounded" minOccurs="0" name="SinglePremium">
                        <xs:complexType>
                          <xs:attribute name="upToPremium"/>
                          <xs:attribute name="charge"/>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="GeneralRPFRChargeUsed" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>Used mainly for PRSAs to show the fund related charge used during the quote. Only works for RP Allocation Code bassed products. </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="GeneralSPFRChargeUsed" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>Used mainly for PRSAs to show the fund related charge used during the quote. Only works for SP Allocation Code bassed products. </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="GeneralRPAllocationUsed" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>Used mainly for PRSA to show the RP allocation used</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="GeneralSPAllocationUsed" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>Used for PRSA to show SP allocation used</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="ReviewInfo" type="generaltypes:ProtectionReviewInfoType">
            <xs:annotation>
              <xs:documentation>Contains information about protection policy reviews (e.g. pass or fail and if fail how the policy can be updated to sustain</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
      <xs:complexType name="DisclosureInfo">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" name="YearRow">
            <xs:complexType>
              <xs:attribute name="year" type="xs:double" use="required">
                <xs:annotation>
                  <xs:documentation>Year for which end of year values are provided in this row.</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="cumulativePremium" type="generaltypes:FormattedDouble" use="required">
                <xs:annotation>
                  <xs:documentation>This is the cumulative total premium to be paid - i.e. not just the change amount
</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="encashmentBeforeTax" type="generaltypes:FormattedDouble" use="required">
                <xs:annotation>
                  <xs:documentation>Calculated policy encashment value before tax at the end of the year represented by this row.</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="encashmentAfterTax" type="generaltypes:FormattedDouble" use="required">
                <xs:annotation>
                  <xs:documentation>Calculated policy encashment value after tax at the end of the year represented by this row.</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="encashmentWithoutChargesBeforeTax" type="generaltypes:FormattedDouble" use="optional"/>
              <xs:attribute name="cumulativeTax" type="generaltypes:FormattedDouble" use="optional"/>
              <xs:attribute name="cumulativeNetWithdrawals" type="generaltypes:FormattedDouble" use="optional"/>
              <xs:attribute name="cumulativeGrossWithdrawals" type="generaltypes:FormattedDouble" use="optional"/>
              <xs:attribute name="cumulativeExpenses" type="generaltypes:FormattedDouble" use="required"/>
              <xs:attribute name="investmentGrowth" type="generaltypes:FormattedDouble" use="required"/>
              <xs:attribute name="cumulativeRiskCost" type="generaltypes:FormattedDouble" use="optional"/>
              <xs:attribute name="grossIncomeYearly" type="generaltypes:FormattedDouble" use="optional">
                <xs:annotation>
                  <xs:documentation>Total amount taken from the policy before tax for the year represented by this row.</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="netIncomeYearly" type="generaltypes:FormattedDouble" use="optional">
                <xs:annotation>
                  <xs:documentation>Total amount taken from the policy after tax for the year represented by this row.</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="taxPaid" type="generaltypes:FormattedDouble" use="optional">
                <xs:annotation>
                  <xs:documentation>Value used for SSIA policies and is the amount of tax paid from the policy when the policy moves from the tax credit system to a nortmal policy </xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="bonus" type="generaltypes:FormattedDouble" use="optional">
                <xs:annotation>
                  <xs:documentation>Accumulated bonus paid to the policy from policy inception to the end of the year represented by this row.</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="yearlyPremium" type="generaltypes:FormattedDouble" use="optional">
                <xs:annotation>
                  <xs:documentation>Total amount of single, regular and transfer premiums paid into the policy for the year represented by this row.</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="costOfRemuneration" type="generaltypes:FormattedDouble" use="optional">
                <xs:annotation>
                  <xs:documentation>Amount of sales commisison that will be paid to the policy seller for the year represented by this row.</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="date" type="xs:date" use="required">
                <xs:annotation>
                  <xs:documentation>The date value for EBQ quotes</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="print" type="xs:boolean" use="optional">
                <xs:annotation>
                  <xs:documentation>Used to indicate to the correspondence that this row should be printed</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="costOfRawRemuneration" type="generaltypes:FormattedDouble">
                <xs:annotation>
                  <xs:documentation>Amount of sales commisison excluding uplift that will be paid to the policy seller for the year represented by this row.</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="costOfRawSpRemuneration" type="generaltypes:FormattedDouble"/>
              <xs:attribute name="costOfRawRpRemuneration" type="generaltypes:FormattedDouble"/>
              <xs:attribute name="costOfRawFundRemuneration" type="generaltypes:FormattedDouble"/>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="ReductionInYield">
            <xs:complexType>
              <xs:attribute name="reductionInYield" type="generaltypes:FormattedDouble" use="required"/>
              <xs:attribute name="reducedSavingsGrowth" type="generaltypes:FormattedDouble" use="required"/>
            </xs:complexType>
          </xs:element>
          <xs:element name="AverageGrowthRate" type="xs:double"/>
          <xs:element name="SurrenderValues">
            <xs:annotation>
              <xs:documentation>This node holds the surrender values of the policy on the policy end month (for ease of use). </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:attribute name="grossEMVPolicyEnd" type="xs:double" use="required"/>
              <xs:attribute name="netEMVPolicyEnd" type="xs:double" use="required"/>
            </xs:complexType>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
    </xs:schema>
    <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://nulquoteoutput.framework.domain.projections.bil.ie" xmlns:enums="http://enums.common.framework.domain.projections.bil.ie" xmlns:generaltypes="http://generaltypes.common.framework.domain.projections.bil.ie" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://nulquoteoutput.framework.domain.projections.bil.ie">
      <xs:import namespace="http://enums.common.framework.domain.projections.bil.ie"/>
      <xs:import namespace="http://generaltypes.common.framework.domain.projections.bil.ie"/>
      <xs:element name="NULQuoteOutputRoot" type="NULQuoteOutput"/>
      <xs:complexType name="NULQuoteOutput">
        <xs:annotation>
          <xs:documentation>Data that defines the Non Linked Quotation Output</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="ClientAssumptions">
            <xs:complexType>
              <xs:sequence>
                <xs:element maxOccurs="unbounded" name="LifeDetailsRow">
                  <xs:complexType>
                    <xs:all>
                      <xs:element name="AssumedAgeYears" type="xs:int">
                        <xs:annotation>
                          <xs:documentation>The clients age in whole years used in the calculations.</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element name="AssumedAgeMonths" type="generaltypes:NrOfMonthsRange">
                        <xs:annotation>
                          <xs:documentation>The clients months in addition to age in years used in the calculations.</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element name="AgeNextBirthday" type="xs:int">
                        <xs:annotation>
                          <xs:documentation>The clients age next birthday used when looking up benefit rates.</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element name="AgeNextQuarter" type="xs:double">
                        <xs:annotation>
                          <xs:documentation>Clients age next quarter. The format is:  xx, xx.25, xx.5, xx.75</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                    </xs:all>
                    <xs:attribute name="forLife" type="enums:EnumLife" use="required">
                      <xs:annotation>
                        <xs:documentation>FIX: Add life enum</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="NormalRetirementAge">
                  <xs:annotation>
                    <xs:documentation>If this is a Pension Projection, this field holds the client expected retirement age in years and months</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:attribute name="years" type="xs:int" use="required"/>
                    <xs:attribute name="months" type="xs:int" use="required"/>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element maxOccurs="unbounded" name="Results">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="PolicyInformation">
                  <xs:complexType>
                    <xs:all>
                      <xs:element name="TermInfo">
                        <xs:complexType>
                          <xs:all>
                            <xs:element name="PolicyTerm">
                              <xs:complexType>
                                <xs:attribute name="years" type="xs:int" use="required"/>
                                <xs:attribute name="months" type="xs:int" use="required"/>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="TermRemaining">
                              <xs:annotation>
                                <xs:documentation>Number of years and months remaiining on the policy from the quote month until the policy end month</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                <xs:attribute name="years" type="xs:int" use="required"/>
                                <xs:attribute name="months" type="xs:int" use="required"/>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="OriginalPolicyEndDate" type="xs:dateTime"/>
                          </xs:all>
                        </xs:complexType>
                      </xs:element>
                      <xs:element name="PremiumInformation">
                        <xs:annotation>
                          <xs:documentation>Information about calculated regular and single premium values</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element name="RegularPremium" type="generaltypes:FormattedDouble">
                              <xs:annotation>
                                <xs:documentation>Calculated or supplied regular premium amount.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element minOccurs="0" name="UnratedPremium" type="generaltypes:FormattedDouble">
                              <xs:annotation>
                                <xs:documentation>Value of the regular premium if ratings were not applied. </xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element name="NextPremiumPaymentDate" type="xs:date"/>
                            <xs:element name="ValidUntil" type="xs:date"/>
                            <xs:element minOccurs="0" name="NetRegularPremium" type="generaltypes:FormattedDouble">
                              <xs:annotation>
                                <xs:documentation>This is the value of ORegularPremium with taxation applied. This is used on pensions to show the actual amount the policy owner would have to pay after tax.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element minOccurs="0" name="RegularPremiumTaxRelief" type="generaltypes:FormattedDouble">
                              <xs:annotation>
                                <xs:documentation>This is ORegularPremium - ONetRegularPremium</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="unbounded" minOccurs="0" name="FuturePremiums">
                              <xs:annotation>
                                <xs:documentation>Used for future term quotes (usually for the next 2 quarters)</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                <xs:attribute name="name" type="xs:string" use="required"/>
                                <xs:attribute name="premium" type="generaltypes:FormattedDouble" use="required"/>
                                <xs:attribute name="validUntil" type="xs:date" use="required"/>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="GovernmentLevyFactor" type="xs:double">
                              <xs:annotation>
                                <xs:documentation>This factor is used to display any premium value inclusive of government levy</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element name="IsIndexing" type="xs:boolean">
                              <xs:annotation>
                                <xs:documentation>True if this regular premium is indexing.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element name="PaymentFrequency" type="enums:EnumFrequency"/>
                          </xs:sequence>
                        </xs:complexType>
                      </xs:element>
                    </xs:all>
                  </xs:complexType>
                </xs:element>
                <xs:element name="RiskBenefitCosts">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" name="RiskCost">
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element maxOccurs="unbounded" minOccurs="0" name="RatingCost">
                              <xs:complexType>
                                <xs:attribute name="forLife" type="enums:EnumLife" use="required"/>
                                <xs:attribute name="ratingFreeCost" type="xs:double" use="required"/>
                                <xs:attribute name="perMilleRating" type="xs:double" use="optional"/>
                                <xs:attribute name="perMilleRatingExtra" type="xs:double"/>
                                <xs:attribute name="perMilleTermYears" type="xs:int" use="optional"/>
                                <xs:attribute name="perMilleTermYearsExtra" type="xs:int"/>
                                <xs:attribute name="perMilleCost" type="xs:double" use="optional"/>
                                <xs:attribute name="perMilleCostExtra" type="xs:double" use="optional"/>
                                <xs:attribute name="percentRating" type="xs:double" use="optional"/>
                                <xs:attribute name="percentCost" type="xs:double" use="optional"/>
                              </xs:complexType>
                            </xs:element>
                          </xs:sequence>
                          <xs:attribute name="forLife" type="enums:EnumLife" use="required"/>
                          <xs:attribute name="benefitId" type="enums:EnumRiskBenefit" use="required"/>
                          <xs:attribute name="benefitName" type="xs:string" use="required"/>
                          <xs:attribute name="sumAssured" type="generaltypes:FormattedDouble" use="required"/>
                          <xs:attribute name="yearOneGrossCost" type="generaltypes:FormattedDouble" use="required"/>
                          <xs:attribute name="unratedYearOneGrossCost" type="generaltypes:FormattedDouble" use="optional"/>
                          <xs:attribute name="term" type="xs:int" use="required"/>
                          <xs:attribute name="endDate" type="xs:dateTime" use="optional"/>
                          <xs:attribute name="benefitTermWasAgeCapped" type="xs:boolean" use="optional"/>
                          <xs:attribute name="ageCapUsed" type="xs:int" use="optional"/>
                          <xs:attribute name="underwritingStatus" type="xs:string" use="optional"/>
                          <xs:attribute name="cessationAge" type="xs:int" use="optional">
                            <xs:annotation>
                              <xs:documentation>This is the age of the client when the risk benefit ceases</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="isJoint" type="xs:boolean" use="optional"/>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
              <xs:attribute name="dateOfChange" type="xs:dateTime" use="optional"/>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="Disclosure">
            <xs:complexType>
              <xs:sequence>
                <xs:element maxOccurs="unbounded" name="YearRow">
                  <xs:complexType>
                    <xs:attribute name="year" type="xs:int" use="required">
                      <xs:annotation>
                        <xs:documentation>Year for which end of year values are provided in this row.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="alterationYear" type="xs:int" use="optional"/>
                    <xs:attribute name="cumulativePremiumAmount" type="generaltypes:FormattedDouble" use="required">
                      <xs:annotation>
                        <xs:documentation>Total regular, single and transfer premium amounts paid from policy inception to the end of the year represented by this row.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="projectedTotalBenefitCost" type="generaltypes:FormattedDouble" use="optional">
                      <xs:annotation>
                        <xs:documentation>Total cost of benefits (i.e. amount taken for fund to pay for risk benefits) from policy inception to the end of the year represented by this row.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="projectedExpenseCharges" type="generaltypes:FormattedDouble" use="required">
                      <xs:annotation>
                        <xs:documentation>Calculated charges due to expenses from policy inception to the end of the year represented by this row.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="yearlyPremium" type="generaltypes:FormattedDouble" use="optional">
                      <xs:annotation>
                        <xs:documentation>Total amount of single, regular and transfer premiums paid into the policy for the year represented by this row.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="remuneration" type="generaltypes:FormattedDouble" use="required"/>
                    <xs:attribute name="yearEndDate" type="xs:dateTime" use="optional"/>
                    <xs:attribute name="print" type="xs:boolean">
                      <xs:annotation>
                        <xs:documentation>Used to indicate to the correspondence that this row should be printed</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="rawRemuneration" type="generaltypes:FormattedDouble" use="optional">
                      <xs:annotation>
                        <xs:documentation>remuneration without uplift</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="BasisUsed" type="generaltypes:BasisRowDef">
            <xs:annotation>
              <xs:documentation>This was the basis info used to produce these results</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="AlterationsConstituteATopup" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>This value will be set for EBQ cases. It will be true if any alterations to the policy constitute a topup, i.e. any increase in regular premium, any number of sp's added to the policy or indexation has been added where it previously wasn't on the policy. </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="NULBenefitPlanType" type="enums:EnumNULBenefitPlanType">
            <xs:annotation>
              <xs:documentation>This value will be set for NBQ cases.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:complexType>
    </xs:schema>
