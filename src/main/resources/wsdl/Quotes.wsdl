                                          <xs:documentation>Used to indicate whether this is a normal Single Premium or a Transfer amount</xs:documentation>
                                        </xs:annotation>
                                      </xs:attribute>
                                      <xs:attribute name="commissionRate" type="xs:double" use="optional"/>
                                      <xs:attribute name="fundCommissionRate" type="xs:double" use="optional"/>
                                      <xs:attribute default="false" name="target" type="xs:boolean" use="optional"/>
                                    </xs:complexType>
                                  </xs:element>
