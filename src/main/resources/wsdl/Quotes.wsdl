                        </xs:annotation>
                      </xs:element>
                      <xs:element minOccurs="0" name="DocEffectiveDate" type="xs:dateTime"/>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="InstructionsForPolicyIssue" type="xs:string">
