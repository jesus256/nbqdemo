          <xs:element name="QuoteTermForNonPensions" type="xs:int">
            <xs:annotation>
              <xs:documentation>This is the term in months to be quoteds for non pension products where the calling application has not supplied a policy end date or a quote end date.</xs:documentation>
            </xs:annotation>
          </xs:element>
