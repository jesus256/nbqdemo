                <xs:element name="PolicyTerm">
                  <xs:annotation>
                    <xs:documentation>Term of the policy in Years and Months</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:attribute name="years" type="xs:int" use="required"/>
                    <xs:attribute name="months" type="xs:int" use="required"/>
                    <xs:attribute name="retirementAge" type="xs:int" use="required"/>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="InitialBillingFrequency" type="enums:EnumFrequency"/>
                <xs:element name="ConversionIndicator" type="xs:boolean"/>
                <xs:element name="ConvertedArf" type="xs:boolean"/>
              </xs:all>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="CurrentPosition">
            <xs:annotation>
              <xs:documentation>Represents the current values of an existing policy - i.e. not required for new business quotation</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:all>
                <xs:element name="PolicyStatus">
                  <xs:annotation>
                    <xs:documentation>Status for this policy (including both policy and premium statuses).  </xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:attribute name="policyStatus" type="enums:EnumPolicyStatus" use="required">
                      <xs:annotation>
                        <xs:documentation>Status of the policy at the time of this history row. </xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="as400PolicyStatus" type="xs:string" use="optional">
                      <xs:annotation>
                        <xs:documentation>Original AS400 policy status. </xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="premiumStatus" type="enums:EnumPremiumStatus" use="required">
                      <xs:annotation>
                        <xs:documentation>Status of the premium at the time of this history row.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="riskBenefitsContinuingInd" type="xs:boolean" use="optional">
                      <xs:annotation>
                        <xs:documentation>This value is only applicable when the policy is PUPed.  it indicates whether a risk benefits are to remain active during the PUPed       period.     </xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="policyRevivalDate" type="xs:dateTime" use="optional">
                      <xs:annotation>
                        <xs:documentation>Records the date the policy was revivied from (if it has been revived)</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:complexType>
                </xs:element>
                <xs:element name="PolicyValues">
                  <xs:annotation>
                    <xs:documentation>Current values of the policy as supplied by the administration system</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:attribute name="grossPolicyValue" type="xs:double" use="required">
                      <xs:annotation/>
                    </xs:attribute>
                    <xs:attribute name="grossPolicyValueIncUnprocTrans" type="xs:double" use="required">
                      <xs:annotation>
                        <xs:documentation>This is the gross policy value calculated using any unprocessed transactions. Note: This excludes unprocessed UTRNs created because   of fund switches and those with a monies date greater than the effective date.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="netPolicyValue" type="xs:double" use="required">
                      <xs:annotation>
                        <xs:documentation>Monatory value of the policy minus surrender penalties and tax. If this value is not available it will be calculated from the other policyinformation provided.    </xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="surrenderPenalty" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>Value of any surrender penalties that accompany the stated surrender value</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="exitTax" type="xs:double" use="optional"/>
                    <xs:attribute name="guaranteeAmount" type="xs:double" use="optional"/>
                    <xs:attribute name="guaranteeEndDate" type="xs:dateTime" use="optional"/>
                    <xs:attribute name="guaranteedAmountNoCalcFlag" type="xs:string" use="optional"/>
                    <xs:attribute name="bonusUnits" type="xs:double" use="optional"/>
                    <xs:attribute name="totalGovLevyPaidToDate" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>CPC Phase3 Value of Gov Levy paid to date</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="revisedRpAmtbeforeGovLevy" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>Required for Indexation Letters - RP amt before Gov Levy</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="govLevyAmtOnRevisedRpPrem" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>Required for Indexation Letters - Gov Levy on RP</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="revisedRpAmtIncludingGovLevy" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>Required for Indexation Letters - RP amt incl Gov Levy</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="indicativeValue" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>Required for Docs</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:complexType>
                </xs:element>
                <xs:element name="BlissPolicyValues">
                  <xs:annotation>
                    <xs:documentation>Current values specific to Bliss policies</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:attribute name="totalCapUnitValue" type="xs:double" use="optional"/>
                    <xs:attribute name="totalAccUnitValue" type="xs:double" use="optional"/>
                    <xs:attribute name="contributionsNotAllocated" type="xs:double" use="optional"/>
                    <xs:attribute name="debtOnPolicy" type="xs:double" use="optional"/>
                    <xs:attribute name="outstandingContributions" type="xs:double" use="optional"/>
                    <xs:attribute name="discCapUnitValue" type="xs:double" use="optional"/>
                    <xs:attribute name="unitElementFlag" type="xs:string" use="optional"/>
                    <xs:attribute name="inNonForfeiturePeriod" type="xs:string" use="optional"/>
                    <xs:attribute name="capUnitsConversionDate" type="xs:dateTime" use="optional"/>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="PolicyDebt">
                  <xs:annotation>
                    <xs:documentation>Records any outstanding debt on this policy. </xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:attribute name="totalFeeDebt" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>The amount of debt outstanding due to admin charges, i.e. AC - ACP
</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="totalRiskDebt" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>The amount of debt outstanding due to commission charges on Open Plan policies</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="employerFeeDebt" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>The amount of debt outstanding on a policy.  This value is not applicable to Open Plan policies.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="employerRiskDebt" type="xs:double" use="optional"/>
                  </xs:complexType>
                </xs:element>
                <xs:element name="PriceDate" type="xs:dateTime">
                  <xs:annotation>
                    <xs:documentation>This is either the single price date for the policy funds or the latest price date if there are multuple dates across the funds. </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="Units">
                  <xs:annotation>
                    <xs:documentation>Current value as units and prives</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" name="FundRow">
                        <xs:annotation>
                          <xs:documentation>1 row per fund held on this policy.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:attribute name="fundId" type="xs:string" use="required">
                            <xs:annotation>
                              <xs:documentation>Identifier for the fund</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="nrEmployeeAccUnits" type="xs:double" use="required">
                            <xs:annotation>
                              <xs:documentation>Number of accumulation units held by the policy in this fund.</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="nrEmployerAccUnits" type="xs:double" use="optional">
                            <xs:annotation>
                              <xs:documentation>Only applicable to policies that are charged using capital units. It holds the number of capital units help by the policy in  this        fund. </xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="nrAVCAccUnits" type="xs:double" use="optional">
                            <xs:annotation>
                              <xs:documentation>Number of units which belong to the Employer contribution category. The number of Employee units can be   calculated by       subtracting the Employer units from the nrAccUnits.</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="priceDate" type="xs:dateTime" use="required"/>
                          <xs:attribute name="currentBidPrice" type="xs:double" use="required">
                            <xs:annotation>
                              <xs:documentation>Bid price of units on the Price Date (price date in held in the PolicyDates node).</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="currentOfferPrice" type="xs:double" use="required">
                            <xs:annotation>
                              <xs:documentation>Offer price of units on the Price Date (price date in held in the PolicyDates node).</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="bonusFund" type="xs:boolean" use="optional">
                            <xs:annotation>
                              <xs:documentation>If true, indicates that this is a fund that holds bonus units.</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="fundName" type="xs:string" use="optional"/>
                          <xs:attribute name="rolledUpFundManagementCharge" type="xs:double" use="optional"/>
                          <xs:attribute name="fundType" type="enums:EnumFundType" use="optional"/>
                          <xs:attribute name="amountInOriginalCurrency" type="xs:double" use="optional"/>
                          <xs:attribute name="capNumberOfUnits" type="xs:double" use="optional">
                            <xs:annotation>
                              <xs:documentation>No of capital units - BLISS only field</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="capAmountInOriginalCurrency" type="xs:double" use="optional">
                            <xs:annotation>
                              <xs:documentation>Amt of capital units - BLISS only field</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="capUnitBidPrice" type="xs:double" use="optional">
                            <xs:annotation>
                              <xs:documentation>Bid price of capital units - BLISS only field</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="capUnitOfferPrice" type="xs:double" use="optional">
                            <xs:annotation>
                              <xs:documentation>Unit offer price of capital units - BLISS only field</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="cashFundIndicator" type="xs:boolean" use="optional">
                            <xs:annotation>
                              <xs:documentation>Is this a cash fund, required by Docs for text on Exec Pensions</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element name="TaxFreePremiumUsed" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>This is the amount of tax free premium used to date, i.e. it is a total of all part surrenders and withdrawls from the policy since it's inception.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="ExtractDate" nillable="true" type="xs:dateTime">
                  <xs:annotation>
                    <xs:documentation>Date that the system extracted this information from the administration system.     </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="TotalPremiumsPaid">
                  <xs:complexType>
                    <xs:attribute name="totalAllPremiumsPaid" type="xs:double" use="required"/>
                    <xs:attribute name="totalRegularPremiumsPaid" type="xs:double" use="required"/>
                    <xs:attribute name="totalSinglePremiumsPaid" type="xs:double" use="required"/>
                    <xs:attribute name="totalTransferPremiumsPaid" type="xs:double" use="optional"/>
                    <xs:attribute name="totalSinglePremiumWithoutTransfer" type="xs:double" use="optional"/>
                    <xs:attribute name="totalRpPremInclGovLevy" type="xs:double" use="optional"/>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="NextFRCActionDate" type="xs:dateTime">
                  <xs:annotation>
                    <xs:documentation>Date that the next Fund Related Charge is due to be taken.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="NextChargingDate" type="xs:dateTime">
                  <xs:annotation>
                    <xs:documentation>Date that the next set of policy charges are due to be taken.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="SPHistory">
                  <xs:annotation>
                    <xs:documentation>Record of any SP's paid onto the policy. This information is ued by the engine to calculat surrender penalties in some cases.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" name="SPHistoryRow">
                        <xs:annotation>
                          <xs:documentation>Record of any SP's paid onto the policy. This information is ued by the engine to calculat surrender penalties in some cases.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:attribute name="effectiveDate" type="xs:dateTime" use="required">
                            <xs:annotation>
                              <xs:documentation>Date of single premium injection.</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="amount" type="xs:double" use="required"/>
                          <xs:attribute name="contributionType" type="enums:EnumContType" use="required">
                            <xs:annotation>
                              <xs:documentation>Used to indicate whether this is a normal Single Premium or a Transfer amount</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element name="TotalWithdrawals">
                  <xs:complexType>
                    <xs:attribute name="totalAllWithdrawalsGross" type="xs:double" use="required"/>
                    <xs:attribute name="totalPartSurrendersPaidGross" type="xs:double" use="required"/>
                    <xs:attribute name="totalIncomePaidGross" type="xs:double" use="required"/>
                    <xs:attribute name="totalAllWithdrawalsNet" type="xs:double" use="required"/>
                    <xs:attribute name="totalPartSurrendersPaidNet" type="xs:double" use="required"/>
                    <xs:attribute name="totalIncomePaidNet" type="xs:double" use="required"/>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="NextReviewDate" type="xs:dateTime">
                  <xs:annotation>
                    <xs:documentation>Next date of review (for unit linked protection policies only)</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="LastPremiumPaidDate" type="xs:dateTime">
                  <xs:annotation>
                    <xs:documentation>This is the date of payment of the last regular premium</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:all>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="RegularPremiumInfo">
            <xs:complexType>
              <xs:all>
                <xs:element name="PremDetails">
                  <xs:complexType>
                    <xs:attribute name="totalPremium" type="xs:double" use="required">
                      <xs:annotation>
                        <xs:documentation>Total regular premium amount being paid to the policy each period.  If there are employer and an  employee contributions, this field will record the addition of both amounts.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="employerPremium" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>Amount of the regular premium being paid by the employee. This is only applicable to pension products. 
            </xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="employeePremium" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>Amount of the regular premium being paid by the employee. This is only applicable to pension products. 
            </xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="avcPremium" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>Amount of AVC premium being paid into the policy each period. </xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="paymentFrequency" type="enums:EnumFrequency" use="required"/>
                    <xs:attribute name="paymentMethod" type="enums:EnumPaymentMode" use="required"/>
                    <xs:attribute name="extraPremium" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>Amount of premium rating being paid into the policy each period.  This is a risk benefit rating method used   on      some products. </xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="overridePercentage" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>Allocation rate override.  If the policy has an increaded allocation due to a reduction in commission, this   field   will    contain the current increaded allocation rate. </xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="reducedAllocApplies" type="xs:boolean" use="optional"/>
                    <xs:attribute name="chosenContribMethod" type="enums:EnumContributionMethod" use="optional">
                      <xs:annotation>
                        <xs:documentation>This value has no affect on the calculation.  This is the user supplied method for specifying their regular premium</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:complexType>
                </xs:element>
                <xs:element name="RPCommission">
                  <xs:complexType>
                    <xs:attribute name="initialCommissionFactor" type="xs:double" use="required"/>
                    <xs:attribute name="initialCommissionMax" type="xs:double" use="required"/>
                    <xs:attribute name="initialCommissionRate" type="xs:double" use="required"/>
                    <xs:attribute name="renewalCommFactor" type="xs:double" use="required"/>
                    <xs:attribute name="fundCommissionRate" type="xs:double" use="optional"/>
                    <xs:attribute name="commissionBasis" type="xs:string" use="optional">
                      <xs:annotation>
                        <xs:documentation>Optional RP commission structure name (used on PRSA)</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="TrancheInfo">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" name="INCIRow">
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element maxOccurs="unbounded" name="InnerRecord">
                              <xs:complexType>
                                <xs:attribute name="SequenceNumber" type="xs:int" use="required"/>
                                <xs:attribute name="PremiumRequiredAtStart" type="xs:double" use="required"/>
                                <xs:attribute name="CurrentOutstanding" type="xs:double" use="required"/>
                                <xs:attribute name="AllocationPercentage" type="xs:double" use="optional"/>
                              </xs:complexType>
                            </xs:element>
                          </xs:sequence>
                          <xs:attribute name="SequenceNumber" type="xs:int" use="required"/>
                          <xs:attribute name="ValidIndicator" type="xs:boolean" use="required"/>
                          <xs:attribute name="OriginalPremium" type="xs:double" use="required"/>
                          <xs:attribute name="CurrentPremium" type="xs:double" use="required"/>
                          <xs:attribute name="RecordCreatedDate" type="xs:dateTime" use="required"/>
                          <xs:attribute name="InitialCommissionFactor" type="xs:double" use="optional"/>
                          <xs:attribute name="MaxInitialCommission" type="xs:double" use="optional"/>
                          <xs:attribute name="InitialCommissionUplift" type="xs:double" use="optional"/>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element name="CurrentPremiumFundSplit">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" name="FundRow">
                        <xs:complexType>
                          <xs:attribute name="fundId" type="xs:string" use="required"/>
                          <xs:attribute name="nbqFundName" type="xs:string" use="optional"/>
                          <xs:attribute name="posFundId" type="xs:string" use="optional"/>
                          <xs:attribute name="fundName" type="xs:string" use="optional"/>
                          <xs:attribute name="percentage" type="xs:double" use="required">
                            <xs:annotation>
                              <xs:documentation>Percentage of the premium being directed to this fund on allocation.</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="rolledUpFundManagementCharge" type="xs:double" use="required"/>
                          <xs:attribute name="hasBeenCompleted" type="xs:boolean" use="required"/>
                          <xs:attribute name="fundType" type="enums:EnumFundType" use="optional"/>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="DecJanPremSkip" type="xs:boolean">
                  <xs:annotation>
                    <xs:documentation>This field indicates whether or not the December and January premiums are to be skipped. </xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:all>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="BenefitInfo">
            <xs:annotation>
              <xs:documentation>Information about Risk Benefits. </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="CoverType" type="enums:EnumNBQCoverType">
                  <xs:annotation>
                    <xs:documentation>Specifies cover type of the Policy.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element maxOccurs="unbounded" name="RiskBenefits" type="generaltypes:RiskBenefit">
                  <xs:annotation>
                    <xs:documentation>There is one of these nodes for each risk benefit on the policy. </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="InclusiveOfFund" type="enums:EnumNBQInclOfFund">
                  <xs:annotation>
                    <xs:documentation>Stores actual value enetered by the user.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="ContributionInclusiveOfRiskBenefits" type="xs:boolean">
                  <xs:annotation>
                    <xs:documentation>Specifies contribution inclusive of risk benefits.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="UnderwrittenInd" type="xs:boolean">
                  <xs:annotation>
                    <xs:documentation>Indicated whether or not this policy has been underwritten.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="PolicyOptionsUWResult" type="generaltypes:PolOptionsUWResult">
                  <xs:annotation>
                    <xs:documentation>If underwriting has been performed on this policy the UW result of Indexation and conversion is recorded here. </xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="Income">
            <xs:annotation>
              <xs:documentation>Information about any regular income on the policy.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element maxOccurs="unbounded" name="IncomeRow">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" minOccurs="0" name="FundRow">
                        <xs:annotation>
                          <xs:documentation>If there is information about the fund split of the withdrawal it can be placed here.  The amount relates to the fund split type held at income row level.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:attribute name="fundId" type="xs:string" use="required"/>
                          <xs:attribute name="amount" type="xs:double" use="required"/>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                    <xs:attribute name="startDate" type="xs:dateTime" use="optional">
                      <xs:annotation>
                        <xs:documentation>Date this withdrawal record became effective</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="endDate" type="xs:dateTime" use="optional">
                      <xs:annotation>
                        <xs:documentation>Date this withdrawal record finished or will finish in the future. If this value is not supplied it will be assumed that this         record  is valid until the end of the polciy. </xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="frequency" type="enums:EnumFrequency" use="required">
                      <xs:annotation>
                        <xs:documentation>Frequency of withdrawals from the policy.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="withdrawalType" type="enums:EnumIncomeType" use="required">
                      <xs:annotation>
                        <xs:documentation>Indicates the method for calculating the withdrawal amount, e.g. Percentage of Fund, Fixed amount etc.        </xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="amount" type="xs:double" use="required">
                      <xs:annotation>
                        <xs:documentation>Amount of the withdrawal.  Note: this value is dependant on the withdrawal type.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="indexationRate" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>Rate at which the amount indexes each policy anniversary.  This value is only applicable when the withdrawal type     is a        monatory amount.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="charge" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>Monatory amount of charge for each withdrawal, if applicable. </xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="firstPaymentInMonth" type="xs:int" use="optional"/>
                    <xs:attribute name="nextWithdrawalDate" type="xs:dateTime" use="optional"/>
                    <xs:attribute name="finalWithdrawalDate" type="xs:dateTime" use="optional"/>
                    <xs:attribute name="isGrossAmount" type="xs:boolean" use="optional"/>
                    <xs:attribute name="fundSplitType" type="enums:EnumFundSplitType" use="optional"/>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="PolicyCharges">
            <xs:complexType>
              <xs:all>
                <xs:element minOccurs="0" name="PolicyFeeOverride">
                  <xs:annotation>
                    <xs:documentation>Value of the policy fee is different from that defined on the product definition.  The quotation will use this value as the starting policy fee and index it based on the product definition rules.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:attribute name="feeOverride" type="xs:double" use="required"/>
                    <xs:attribute name="isOverriden" type="xs:boolean" use="required"/>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="PensionBoardFee" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>This is a specific node for the pension board fee.  It is handled slightly differently than other fees and therefore  requires special treatment</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="NonSurrenderValueEndDate" type="xs:dateTime">
                  <xs:annotation>
                    <xs:documentation>Date at which the Non Surrender Value Period Ends for a Policy</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="ChargingAdjustments">
                  <xs:annotation>
                    <xs:documentation>Information about the charging structure on this policy. This structure is used primarily by AS400 products.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:all>
                      <xs:element default="true" minOccurs="0" name="IncludeSurrenderPenalties" type="xs:boolean">
                        <xs:annotation>
                          <xs:documentation>Whether or not surrender penalties are required on the policy. </xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element minOccurs="0" name="FRCOverride">
                        <xs:annotation>
                          <xs:documentation>Used to override the FRC percentage on the product definition</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:attribute name="regPremiumOverride" type="xs:double" use="optional"/>
                          <xs:attribute name="singlePremiumOverride" type="xs:double" use="optional"/>
                        </xs:complexType>
                      </xs:element>
                      <xs:element minOccurs="0" name="FundBasedCommissionRate">
                        <xs:annotation>
                          <xs:documentation>Fund based commission rate chosen by the policy seller.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:attribute name="regPremiumOverride" type="xs:double" use="optional"/>
                          <xs:attribute name="singlePremiumOverride" type="xs:double" use="optional"/>
                        </xs:complexType>
                      </xs:element>
                      <xs:element minOccurs="0" name="FlexCharging">
                        <xs:annotation>
                          <xs:documentation>If the product is flex charging the policy will contain the name of the flex charging structure or will contain the rates themselves. </xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:choice>
                            <xs:element name="StructureName" type="xs:string">
                              <xs:annotation>
                                <xs:documentation>This is the name of the flex charging structure</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element name="Rates">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element maxOccurs="unbounded" name="AllocRateAdjustment">
                                    <xs:complexType>
                                      <xs:attribute name="adjustment" type="xs:double" use="required"/>
                                      <xs:attribute name="upToPremium" type="generaltypes:ValueOrMaxType" use="required"/>
                                    </xs:complexType>
                                  </xs:element>
                                  <xs:element name="FRCOverride" type="xs:double"/>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                          </xs:choice>
                        </xs:complexType>
                      </xs:element>
                      <xs:element minOccurs="0" name="EnhancedAllocationOverride">
                        <xs:complexType>
                          <xs:attribute name="regPremiumOverride" type="xs:double" use="optional"/>
                          <xs:attribute name="singlePremiumOverride" type="xs:double" use="optional"/>
                        </xs:complexType>
                      </xs:element>
                      <xs:element minOccurs="0" name="CommissionGiveup">
                        <xs:annotation>
                          <xs:documentation>Details about commission given up to affect change in the charges / allocation</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:attribute name="commGivenUpToIncAlloc" type="xs:double" use="optional"/>
                          <xs:attribute name="commGivenUpToReduceMgtChg" type="xs:double" use="optional"/>
                        </xs:complexType>
                      </xs:element>
                      <xs:element minOccurs="0" name="FundChargeRate" type="xs:double">
                        <xs:annotation>
                          <xs:documentation>This is the annual Fund Management Charge for Standard funds. A Non Standard fund with an extra 0.25 or 0.35 is in addition of this.</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element minOccurs="0" name="RegularPremiumCharge" type="xs:double">
                        <xs:annotation>
                          <xs:documentation>This is used to calculate the net investment allocation for Regular and Single Contributions.</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                    </xs:all>
                  </xs:complexType>
                </xs:element>
              </xs:all>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="PensionSpecific">
            <xs:complexType>
              <xs:all>
                <xs:element minOccurs="0" name="NRA" type="xs:int">
                  <xs:annotation>
                    <xs:documentation>The clients normal retirement age for thispolicy</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element default="false" minOccurs="0" name="AutomaticFundSwitch" type="xs:boolean">
                  <xs:annotation>
                    <xs:documentation>Determines whether or not the policy is to be automatically switched to less risky funds closer to the policy maturing.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="PensionBoardPrefix" type="xs:string"/>
                <xs:element minOccurs="0" name="PensionBoardNumber" type="xs:string"/>
                <xs:element minOccurs="0" name="RetainedBenefit" type="xs:double"/>
                <xs:element minOccurs="0" name="RetainedBenefitDescriptions">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="5" minOccurs="0" name="RetainedBenefitDescription" type="xs:string"/>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="VestedRights">
                  <xs:complexType>
                    <xs:attribute name="withdrawalRights" type="enums:EnumVestedRights" use="required"/>
                    <xs:attribute name="monthsToObtainRights" type="xs:int" use="optional"/>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="PensionBoardNumberReference" type="xs:string"/>
                <xs:element minOccurs="0" name="RBondFlatFee" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>Retirement Bond flat fee</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="RevFundLimit" type="xs:double"/>
                <xs:element minOccurs="0" name="RevEarningsLimit" type="xs:double"/>
                <xs:element minOccurs="0" name="ExecPensionInfo">
                  <xs:annotation>
                    <xs:documentation>Additional information required on executive pensions</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:all>
                      <xs:element name="DateJoinedService" type="xs:dateTime"/>
                      <xs:element minOccurs="0" name="DateLeftService" type="xs:dateTime"/>
                      <xs:element name="FivePercentDirector" type="xs:boolean"/>
                      <xs:element minOccurs="0" name="LastKnownMaxPercentOfCurrSal" type="xs:double"/>
                      <xs:element name="TwentyPercentDirector" type="xs:boolean"/>
                      <xs:element name="MaxLumpSumEntitlement" type="xs:double"/>
                      <xs:element name="NormalPensionDate" type="xs:dateTime"/>
                    </xs:all>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="LastKnownSalary" type="xs:double"/>
                <xs:element minOccurs="0" name="SchemeRenewalDay" type="xs:int">
                  <xs:annotation>
                    <xs:documentation>required in CPC phase3 for TIP products (YN7)</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="SchemeRenewalMonth" type="xs:int">
                  <xs:annotation>
                    <xs:documentation>required in CPC phase3 for TIP products (YN7)</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="IsVestedPRSA" type="xs:boolean">
                  <xs:annotation>
                    <xs:documentation>Indicates that this PRSA has had the tax free lump sum waived already</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:all>
            </xs:complexType>
          </xs:element>
          <xs:element name="TargetFund">
            <xs:complexType>
              <xs:attribute name="targetFundId" type="xs:string" use="optional"/>
              <xs:attribute name="targetFundDesc" type="xs:string" use="optional"/>
              <xs:attribute name="targetFundAmount" type="xs:double" use="optional"/>
              <xs:attribute name="miniFundId" type="xs:string" use="optional"/>
              <xs:attribute name="miniFundDesc" type="xs:string" use="optional"/>
              <xs:attribute name="miniFundAmount" type="xs:double" use="optional"/>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="NewBusinessSpecific">
            <xs:annotation>
              <xs:documentation>Any policy data elements which are required for New Business Quotes only.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element maxOccurs="2" name="OpeningSinglePremium">
                  <xs:annotation>
                    <xs:documentation>This is used for New Business Quotes. It specifies the opening single premium (either as the main investment or as an RP rider). There can be up to 2 for NBQ (i.e. one SP and one TV)</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" name="FundSplit">
                        <xs:complexType>
                          <xs:attribute name="fundId" type="xs:string" use="required"/>
                          <xs:attribute name="nbqFundName" type="xs:string" use="optional"/>
                          <xs:attribute name="posFundId" type="xs:string" use="optional"/>
                          <xs:attribute name="fundName" type="xs:string" use="optional"/>
                          <xs:attribute name="percentage" type="xs:double" use="required">
                            <xs:annotation>
                              <xs:documentation>Percentage of the premium being directed to this fund on allocation.</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="rolledUpFundManagementCharge" type="xs:double" use="required"/>
                          <xs:attribute name="hasBeenCompleted" type="xs:boolean" use="required"/>
                          <xs:attribute name="fundType" type="enums:EnumFundType" use="optional"/>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                    <xs:attribute name="amount" type="xs:double" use="required"/>
                    <xs:attribute name="investmentOverridePercent" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>Allocation rate this single premium was allocated to the policy with. </xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="agencyNumber" type="xs:string" use="optional">
                      <xs:annotation>
                        <xs:documentation>Agency Number of the ageny who brough this new business to the policy if different to the Agency who sold the policy. </xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="contributionType" type="enums:EnumContType" use="required">
                      <xs:annotation>
                        <xs:documentation>Used to indicate whether this is a normal Single Premium or a Transfer amount</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="commissionRate" type="xs:double" use="optional"/>
                    <xs:attribute default="false" name="target" type="xs:boolean" use="optional"/>
                    <xs:attribute name="employerPercentage" type="xs:double" use="optional"/>
                    <xs:attribute name="singlePremiumCharge" type="xs:double" use="optional"/>
                    <xs:attribute name="commissionBasis" type="xs:string" use="optional">
                      <xs:annotation>
                        <xs:documentation>Optional SP commission structure name (used on PRSA)</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="prsaSpCommission" type="xs:string" use="optional"/>
                    <xs:attribute name="amrfAmount" type="xs:double">
                      <xs:annotation>
                        <xs:documentation>required for FBD</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="arfAmount" type="xs:double">
                      <xs:annotation>
                        <xs:documentation>required for FBD</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:all>
      </xs:complexType>
    </xs:schema>
    <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://nulpolicydata.framework.domain.projections.bil.ie" xmlns:enums="http://enums.common.framework.domain.projections.bil.ie" xmlns:generaltypes="http://generaltypes.common.framework.domain.projections.bil.ie" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://nulpolicydata.framework.domain.projections.bil.ie">
      <xs:import namespace="http://enums.common.framework.domain.projections.bil.ie"/>
      <xs:import namespace="http://generaltypes.common.framework.domain.projections.bil.ie"/>
      <xs:element name="NULPolicyDataRoot" type="NULPolicyData"/>
      <xs:complexType name="NULPolicyData">
        <xs:annotation>
          <xs:documentation>Contains all data elements pertaining only to the policy.</xs:documentation>
        </xs:annotation>
        <xs:all>
          <xs:element name="PolicyDates">
            <xs:annotation>
              <xs:documentation>Policy start/end/cessation dates</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:all>
                <xs:element name="PolicyStartDate" type="xs:dateTime">
                  <xs:annotation>
                    <xs:documentation>Policy start date</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="PolicyEndDate" type="xs:dateTime">
                  <xs:annotation>
                    <xs:documentation>Policy end date</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="PolicyTermInMonths" type="xs:int">
                  <xs:annotation>
                    <xs:documentation>Policy term in months</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="PolicyTerm">
                  <xs:annotation>
                    <xs:documentation>Term of the policy in Years and Months</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:attribute name="years" type="xs:int" use="required"/>
                    <xs:attribute name="months" type="xs:int" use="required"/>
                    <xs:attribute name="retirementAge" type="xs:int" use="required"/>
                  </xs:complexType>
                </xs:element>
              </xs:all>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="CurrentPosition">
            <xs:annotation>
              <xs:documentation>Represents the current values of an existing policy - i.e. not required for new business quotation</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:all>
                <xs:element minOccurs="0" name="TotalPremiumsPaid">
                  <xs:complexType>
                    <xs:attribute name="totalRegularPremiumsPaid" type="xs:double" use="required"/>
                  </xs:complexType>
                </xs:element>
                <xs:element name="PolicyStatus">
                  <xs:annotation>
                    <xs:documentation>Used to indicate the status of the policy.  </xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                    <xs:restriction base="enums:EnumPolicyStatus"/>
                  </xs:simpleType>
                </xs:element>
                <xs:element name="AS400PolicyStatus">
                  <xs:annotation>
                    <xs:documentation>Used to indicate original AS400 policy status.  </xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                    <xs:restriction base="xs:string"/>
                  </xs:simpleType>
                </xs:element>
                <xs:element name="PremiumStatus">
                  <xs:annotation>
                    <xs:documentation>Used to indicate the status of the premium.  </xs:documentation>
                  </xs:annotation>
                  <xs:simpleType>
                    <xs:restriction base="enums:EnumPremiumStatus"/>
                  </xs:simpleType>
                </xs:element>
                <xs:element minOccurs="0" name="PolicyValues">
                  <xs:annotation>
                    <xs:documentation>Current values of the policy as supplied by the administration system</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:attribute name="ETACoverAmount" type="xs:double" use="required"/>
                    <xs:attribute name="revisedRpAmtbeforeGovLevy" type="xs:double" use="optional"/>
                    <xs:attribute name="govLevyAmtOnRevisedRpPrem" type="xs:double" use="optional"/>
                    <xs:attribute name="revisedRpAmtIncludingGovLevy" type="xs:double" use="optional"/>
                    <xs:attribute name="indicativeValue" type="xs:double" use="optional"/>
                  </xs:complexType>
                </xs:element>
              </xs:all>
            </xs:complexType>
          </xs:element>
          <xs:element name="BenefitInfo">
            <xs:annotation>
              <xs:documentation>Information about Risk Benefits. </xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="CoverType" type="enums:EnumNBQCoverType">
                  <xs:annotation>
                    <xs:documentation>Specifies cover type of the Policy.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="Term" type="xs:int">
                  <xs:annotation>
                    <xs:documentation>Specifies length of the term for benefits.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="MonthlyIncomeBenefitsTerm" type="xs:int">
                  <xs:annotation>
                    <xs:documentation>Specifies length of the monthly income term for benefits.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element maxOccurs="unbounded" name="RiskBenefits" type="generaltypes:RiskBenefit">
                  <xs:annotation>
                    <xs:documentation>There is one of these nodes for each risk benefit on the policy. </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="InclusiveOfFund" type="enums:EnumNBQInclOfFund">
                  <xs:annotation>
                    <xs:documentation>Stores actual value enetered by the user.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="ContributionInclusiveOfRiskBenefits" type="xs:boolean">
                  <xs:annotation>
                    <xs:documentation>Specifies contribution inclusive of risk benefits.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="UnderwrittenInd" type="xs:boolean">
                  <xs:annotation>
                    <xs:documentation>Indicated whether or not this policy has been underwritten.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="PolicyOptionsUWResult" type="generaltypes:PolOptionsUWResult">
                  <xs:annotation>
                    <xs:documentation>If underwriting has been performed on this policy the UW result of Indexation and conversion is recorded here. </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="AmountOfIPForPensProt" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>This is the amount of Income protection which the client has decided to use for Pension Contribution cover (used in NBQ fr Exec IP). </xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="RegPremiumDetails">
            <xs:complexType>
              <xs:all>
                <xs:element name="PaymentFrequency" type="enums:EnumFrequency">
                  <xs:annotation>
                    <xs:documentation>Clients desired frequency of payment</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="PaymentMethod" type="enums:EnumPaymentMode">
                  <xs:annotation>
                    <xs:documentation>Clients method of payment (e.g. Direct Debit, Cash, etc)</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="PremAmount" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>Only set on existing policies where the current regular premium payment amount is known</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="IsIndexing" type="xs:boolean">
                  <xs:annotation>
                    <xs:documentation>Indicates whether or not the policy is indexing</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="indexationRate" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>Indexation Rate, required for Indexation Letters.</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:all>
            </xs:complexType>
          </xs:element>
          <xs:element name="ConversionOption" type="xs:boolean"/>
          <xs:element minOccurs="0" name="Commission">
            <xs:annotation>
              <xs:documentation>Commission code associstes with this policy</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:all>
                <xs:element name="ChargingMethod" type="xs:string"/>
                <xs:element name="commissionOption1" type="xs:string"/>
                <xs:element name="commissionOption2" type="xs:string"/>
              </xs:all>
            </xs:complexType>
          </xs:element>
        </xs:all>
      </xs:complexType>
    </xs:schema>
    <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://trackerpolicydata.framework.domain.projections.bil.ie" xmlns:enums="http://enums.common.framework.domain.projections.bil.ie" xmlns:generaltypes="http://generaltypes.common.framework.domain.projections.bil.ie" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://trackerpolicydata.framework.domain.projections.bil.ie">
      <xs:import namespace="http://enums.common.framework.domain.projections.bil.ie"/>
      <xs:import namespace="http://generaltypes.common.framework.domain.projections.bil.ie"/>
      <xs:element name="TrackerPolicyDataRoot" type="TrackerPolicyData">
        <xs:annotation>
          <xs:documentation>Bank of Ireland Life Existing Business Policy Data schema.  </xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:complexType name="TrackerPolicyData">
        <xs:all>
          <xs:element name="PolicyStartDate" type="xs:dateTime">
            <xs:annotation>
              <xs:documentation>Policy start date</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="InvestmentAmt">
            <xs:annotation>
              <xs:documentation>Amount being invested</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:attribute name="totalInvestmentAmount" type="xs:double" use="required">
                <xs:annotation>
                  <xs:documentation>This is the total amount being invested</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="amountOfInvestmentToFixedReturn" type="xs:double" use="optional">
                <xs:annotation>
                  <xs:documentation>This will only be set when the product type contains an optional component. This is the amount of the total investment amount which is going to go to fix return. </xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="oneYearFixed" type="xs:boolean" use="required">
                <xs:annotation>
                  <xs:documentation>Determines if amountOfInvestmentToFixedReturn is investement amount or extra value passed by the user on the screen.</xs:documentation>
                </xs:annotation>
              </xs:attribute>
            </xs:complexType>
          </xs:element>
          <xs:element name="Commission">
            <xs:complexType>
              <xs:sequence>
                <xs:element name="ForInvestment">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element name="CommissionRate" type="xs:double">
                        <xs:annotation>
                          <xs:documentation>This is the rate of commission</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element minOccurs="0" name="CommissionGiveUp">
                        <xs:annotation>
                          <xs:documentation>Records any amount of commission given up to improve the policy return</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:attribute name="giveUpToIncAllocation" type="xs:double" use="optional"/>
                          <xs:attribute name="giveUpToDecMgtCharge" type="xs:double" use="optional"/>
                          <xs:attribute name="giveUpToIncFixedReturn" type="xs:double" use="optional"/>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="ForOptionalFixedRet">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element name="CommissionRate" type="xs:double">
                        <xs:annotation>
                          <xs:documentation>This is the rate of commission</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element minOccurs="0" name="CommissionGiveUp">
                        <xs:annotation>
                          <xs:documentation>Records any amount of commission given up to improve the policy return</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:attribute name="giveUpToIncAllocation" type="xs:double" use="optional"/>
                          <xs:attribute name="giveUpToDecMgtCharge" type="xs:double" use="optional"/>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="EnhancedAllocationPercentage" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>This is the enhanced allocation percentage required for ABS</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="Income">
            <xs:annotation>
              <xs:documentation>Information about any regular income on the policy.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="IncomeRow">
                  <xs:complexType>
                    <xs:attribute name="startDate" type="xs:dateTime" use="optional">
                      <xs:annotation>
                        <xs:documentation>Date this withdrawal record became effective</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="frequency" type="enums:EnumFrequency" use="required">
                      <xs:annotation>
                        <xs:documentation>Frequency of withdrawals from the policy.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="withdrawalType" type="enums:EnumIncomeType" use="required">
                      <xs:annotation>
                        <xs:documentation>Indicates the method for calculating the withdrawal amount, e.g. Percentage of Fund, Fixed amount etc.        </xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="amount" type="xs:double" use="required">
                      <xs:annotation>
                        <xs:documentation>Amount of the withdrawal.  Note: this value is dependant on the withdrawal type.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="indexationRate" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>Rate at which the amount indexes each policy anniversary.  This value is only applicable when the withdrawal type     is a        monatory amount.</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="EarlyEncashmentOption" type="xs:boolean"/>
          <xs:element minOccurs="0" name="ForStaff" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>Indicated whether or not this is a staff case.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="CurrentPosition">
            <xs:annotation>
              <xs:documentation>Represents the current values of an existing policy - i.e. not required for new business quotation</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:all>
                <xs:element name="PolicyValues">
                  <xs:annotation>
                    <xs:documentation>Current values of the policy as supplied by the administration system</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:attribute name="grossPolicyValue" type="xs:double" use="optional">
                      <xs:annotation/>
                    </xs:attribute>
                    <xs:attribute name="netPolicyValue" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>Monetary value of the policy minus surrender penalties and tax. If this value is not available it will be calculated from the other policyinformation provided.    </xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="exitTax" type="xs:double" use="optional"/>
                    <xs:attribute name="guaranteedAmountNoCalcFlag" type="xs:string" use="optional"/>
                    <xs:attribute name="guaranteedAmount" type="xs:double" use="optional"/>
                    <xs:attribute name="indicativeValue" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>Required for Docs</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="netTaxAmountAtOfferPrice" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>Required for ABS Docs</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                    <xs:attribute name="netPolicyValueAtOfferPrice" type="xs:double" use="optional">
                      <xs:annotation>
                        <xs:documentation>Required for ABS docs</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:complexType>
                </xs:element>
                <xs:element name="TotalWithdrawals">
                  <xs:complexType>
                    <xs:attribute name="totalPartSurrendersPaidGross" type="xs:double" use="optional"/>
                    <xs:attribute name="totalIncomePaidGross" type="xs:double" use="optional"/>
                    <xs:attribute name="totalPartSurrendersPaidNet" type="xs:double" use="optional"/>
                    <xs:attribute name="totalIncomePaidNet" type="xs:double" use="optional"/>
                  </xs:complexType>
                </xs:element>
                <xs:element name="Units">
                  <xs:annotation>
                    <xs:documentation>Current value as units and prives</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" name="FundRow">
                        <xs:annotation>
                          <xs:documentation>1 row per fund held on this policy.</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:attribute name="fundId" type="xs:string" use="required">
                            <xs:annotation>
                              <xs:documentation>Identifier for the fund</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="nrEmployeeAccUnits" type="xs:double" use="optional">
                            <xs:annotation>
                              <xs:documentation>Number of accumulation units held by the policy in this fund.</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="nrEmployerAccUnits" type="xs:double" use="optional">
                            <xs:annotation>
                              <xs:documentation>Only applicable to policies that are charged using capital units. It holds the number of capital units help by the policy in  this        fund. </xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="nrAVCAccUnits" type="xs:double" use="optional">
                            <xs:annotation>
                              <xs:documentation>Number of units which belong to the Employer contribution category. The number of Employee units can be   calculated by       subtracting the Employer units from the nrAccUnits.</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="priceDate" type="xs:dateTime" use="optional"/>
                          <xs:attribute name="currentBidPrice" type="xs:double" use="optional">
                            <xs:annotation>
                              <xs:documentation>Bid price of units on the Price Date (price date in held in the PolicyDates node).</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="currentOfferPrice" type="xs:double" use="optional">
                            <xs:annotation>
                              <xs:documentation>Offer price of units on the Price Date (price date in held in the PolicyDates node).</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="bonusFund" type="xs:boolean" use="optional">
                            <xs:annotation>
                              <xs:documentation>If true, indicates that this is a fund that holds bonus units.</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="fundName" type="xs:string" use="optional"/>
                          <xs:attribute name="rolledUpFundManagementCharge" type="xs:double" use="optional"/>
                          <xs:attribute name="fundType" type="enums:EnumFundType" use="optional"/>
                          <xs:attribute name="amountInOriginalCurrency" type="xs:double" use="optional"/>
                          <xs:attribute name="fundValueAtOfferPrice" type="xs:double" use="optional">
                            <xs:annotation>
                              <xs:documentation>Required for ABS docs</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                          <xs:attribute name="cashFundIndicator" type="xs:boolean" use="optional">
                            <xs:annotation>
                              <xs:documentation>Is this a cash fund, required by Docs for text on cover letter</xs:documentation>
                            </xs:annotation>
                          </xs:attribute>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element name="TotalPremiumsPaid">
                  <xs:complexType>
                    <xs:attribute name="totalSinglePremiumsPaid" type="xs:double" use="optional"/>
                  </xs:complexType>
                </xs:element>
              </xs:all>
            </xs:complexType>
          </xs:element>
        </xs:all>
      </xs:complexType>
    </xs:schema>
