                    <xs:sequence>
                      <xs:element minOccurs="0" name="Questionare">
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element name="Responses">
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element maxOccurs="unbounded" name="ClientResponse">
                                    <xs:complexType>
                                      <xs:attribute name="questionId" type="xs:string" use="required"/>
                                      <xs:attribute name="questionText" type="xs:string" use="required"/>
                                      <xs:attribute name="clientResponseId" type="xs:string" use="required"/>
                                      <xs:attribute name="clientResponse" type="xs:string" use="required"/>
                                      <xs:attribute name="responseScore" type="xs:double" use="required"/>
                                    </xs:complexType>
                                  </xs:element>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                            <xs:element name="Result">
                              <xs:complexType>
                                <xs:attribute name="calculatedQuestionareRisk" type="xs:int" use="required"/>
                                <xs:attribute name="mappedESMARiskLevel" type="xs:int" use="required"/>
                                <xs:attribute name="mappedNewIrelandRisk" type="enums:EnumAttitudeToRisk" use="required"/>
                                <xs:attribute name="riskDerivedRate" type="xs:dateTime" use="optional"/>
                              </xs:complexType>
                            </xs:element>
                          </xs:sequence>
