                                <xs:attribute name="asset" type="xs:string" use="required"/>
                                <xs:attribute name="percentage" type="xs:decimal" use="required"/>
                                <xs:attribute name="hexColor" type="xs:string" use="optional"/>
                              </xs:complexType>
                            </xs:element>
                            <xs:element minOccurs="0" name="ReportLocation" type="xs:string">
                              <xs:annotation>
                                <xs:documentation>Gets set if this is sent for report generation</xs:documentation>
                              </xs:annotation>
