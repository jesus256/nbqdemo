      <xs:element name="QuoteScenarioRoot">
        <xs:complexType>
          <xs:sequence>
            <xs:element name="QuoteScenario" type="QuoteScenario"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:complexType name="QuoteScenario">
        <xs:annotation>
          <xs:documentation>Contains all quotation scenario data. This includes growth and indexation assumptions as well as any future development information and  targeting information.</xs:documentation>
        </xs:annotation>
        <xs:all>
          <xs:element name="QuoteDate" type="xs:dateTime">
            <xs:annotation>
              <xs:documentation>This is the date the quote is being performed on.  It is essential that this value is supplied for an EBQ quote. </xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="SpecialTerms" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>Is the quote a special terms quotations?</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element default="false" name="ReviewDetails">
            <xs:annotation>
              <xs:documentation>Is this quote part of a review group of quotes?</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:simpleContent>
                <xs:extension base="xs:boolean">
                  <xs:attribute name="partOfReview" type="xs:boolean" use="required">
                    <xs:annotation>
                      <xs:documentation>True if in review mode.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="docType" type="enums:EnumDocumentSubType" use="required">
                    <xs:annotation>
                      <xs:documentation>Determines which review type document the calculated quote will be archived as</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="currentReviewDate" type="xs:dateTime" use="required">
                    <xs:annotation>
                      <xs:documentation>The current review date</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="wholeOfLifeFollowOn" type="xs:boolean" use="required">
                    <xs:annotation>
                      <xs:documentation>Will a whole of life follow-on quote be required if the review passes.</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="endOfTermReview" type="xs:boolean" use="required">
                    <xs:annotation>
                      <xs:documentation>Is the initial quote an end of term review (i.e a whole of life quote)</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="followOn" type="xs:boolean"/>
                </xs:extension>
              </xs:simpleContent>
            </xs:complexType>
          </xs:element>
          <xs:element name="QuoteType" type="xs:string">
            <xs:annotation>
              <xs:documentation>Value from Quote Type Selector which will be reused when quote is retrieved.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="InputValues" type="inputvalues:InputValues">
            <xs:annotation>
              <xs:documentation>Holds input information about Optionsof the Scenario.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element name="QuoteSettings">
            <xs:annotation>
              <xs:documentation>Either linked or non linked quote settings (basis info, developments etc).</xs:documentation>
            </xs:annotation>
