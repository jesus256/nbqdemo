                <xs:element name="YearlyPremiumDiscountFactor" type="xs:double"/>
                <xs:element name="MinimumAge" type="xs:int"/>
                <xs:element name="AverageYearLengthInDays" type="xs:double"/>
                <xs:element minOccurs="0" name="FutureQuotes">
                  <xs:annotation>
                    <xs:documentation>Tells the Nul Enigne how many future quotes to perform and how many months into the future each one is. </xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" name="FutureQuoteRows">
                        <xs:complexType>
                          <xs:attribute name="name" type="xs:string" use="required"/>
                          <xs:attribute name="nrFutureQuarters" type="xs:int" use="required"/>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element name="DefaultMortgageInterestRate" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>Can be used for Mortgage quotes where there user has not supplied the mortgage interest rate.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="BenefitChargesSmallAdjustment" type="xs:int"/>
                <xs:element name="BenefitChargesLargeAdjustment" type="xs:int"/>
                <xs:element name="MainframeOneTwelfthApproximation" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>This value (currently 0.0833) is used in benefit charges claculation for disclosure</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="MainframeLocalTableAdjustment" type="xs:int">
                  <xs:annotation>
                    <xs:documentation>This value (currently 400) is used in benefit charges claculation for disclosure</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="BankCommissionCutOffDate" type="xs:dateTime">
                  <xs:annotation>
                    <xs:documentation>Commission is calculated differently for LifeChoice policies which begin on or after this date (currently 2011-01-24T00:00:00)</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="BankCommissionCutOffDateCoefficient" type="xs:int">
                  <xs:annotation>
                    <xs:documentation>This coefficient (currently 2) is applied to the rsvp factor and max term to go for LifeChoice policies which begin on or after the cut off date</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="AdditionToRateRatingAdjustment" type="xs:int"/>
                <xs:element name="MaxSurgeryPaymentSA" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>This is used in the calculation of the Surgery Cash Sun Assured. Calcaed as half the crit to a max of this value.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="StateDisabilityBenefitAmount" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>This is a global variable that represents the current amount of State Disability Benefit.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="MarriedStateDisabilityBenefitAmount" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>This is a global variable that represents the current amount of State Disability Benefit for a married person.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="ExecCoverMinimumValue" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>This is a global variable that represents the minimum allowedd value for Executive Cover.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="StateDisabilitySalaryThreshold" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>This is a global variable that represents the current amount of State Disability Salary threshold.</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:all>
            </xs:complexType>
          </xs:element>
          <xs:element name="UnitLinked">
            <xs:complexType>
              <xs:all>
