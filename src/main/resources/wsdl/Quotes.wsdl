                                        <xs:element maxOccurs="unbounded" minOccurs="0" name="FundRow">
                                          <xs:annotation>
                                            <xs:documentation>If fund split is not supplied then part surrender taken in current proportion. </xs:documentation>
                                          </xs:annotation>
                                          <xs:complexType>
                                            <xs:attribute name="fundId" type="xs:string" use="required"/>
                                            <xs:attribute name="percentage" type="xs:double" use="required">
                                              <xs:annotation>
                                                <xs:documentation>Percentage of the premium being directed to this fund on allocation.</xs:documentation>
                                              </xs:annotation>
                                            </xs:attribute>
                                          </xs:complexType>
                                        </xs:element>
                                      </xs:sequence>
                                      <xs:attribute name="amount" type="xs:double" use="required"/>
                                      <xs:attribute name="partEncashmentFee" type="xs:double" use="optional"/>
                                      <xs:attribute default="false" name="target" type="xs:boolean" use="optional"/>
                                      <xs:attribute name="grossOfTax" type="xs:boolean">
                                        <xs:annotation>
                                          <xs:documentation>Is the part surrender gross of tax</xs:documentation>
                                        </xs:annotation>
                                      </xs:attribute>
                                      <xs:attribute name="partSurrenderType" type="enums:EnumIncomeType"/>
                                    </xs:complexType>
                                  </xs:element>
                                </xs:sequence>
                                <xs:attribute name="surrenderDate" type="xs:dateTime" use="required"/>
                              </xs:complexType>
                            </xs:element>
                            <xs:element maxOccurs="unbounded" minOccurs="0" name="Income">
                              <xs:annotation>
                                <xs:documentation>Information about any regular income on the policy.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element name="IncomeRow">
                                    <xs:complexType>
                                      <xs:sequence>
                                        <xs:element maxOccurs="unbounded" minOccurs="0" name="FundRow">
                                          <xs:annotation>
                                            <xs:documentation>If there is information about the fund split of the withdrawal it can be placed here.  The amount relates to the fund split type held at income row level.</xs:documentation>
                                          </xs:annotation>
                                          <xs:complexType>
                                            <xs:attribute name="fundId" type="xs:string" use="required"/>
                                            <xs:attribute name="amount" type="xs:double" use="required"/>
                                          </xs:complexType>
                                        </xs:element>
                                      </xs:sequence>
                                      <xs:attribute name="endDate" type="xs:dateTime" use="optional">
                                        <xs:annotation>
                                          <xs:documentation>Date this withdrawal record finished or will finish in the future. If this value is not supplied it will be assumed that this         record  is valid until the end of the polciy. </xs:documentation>
                                        </xs:annotation>
                                      </xs:attribute>
                                      <xs:attribute name="frequency" type="enums:EnumFrequency" use="required">
                                        <xs:annotation>
                                          <xs:documentation>Frequency of withdrawals from the policy.</xs:documentation>
                                        </xs:annotation>
                                      </xs:attribute>
                                      <xs:attribute name="withdrawalType" type="enums:EnumIncomeType" use="required">
                                        <xs:annotation>
                                          <xs:documentation>Indicates the method for calculating the withdrawal amount, e.g. Percentage of Fund, Fixed amount etc.        </xs:documentation>
                                        </xs:annotation>
                                      </xs:attribute>
                                      <xs:attribute name="amount" type="xs:double" use="required">
                                        <xs:annotation>
                                          <xs:documentation>Amount of the withdrawal.  Note: this value is dependant on the withdrawal type.</xs:documentation>
                                        </xs:annotation>
                                      </xs:attribute>
                                      <xs:attribute name="indexationRate" type="xs:double" use="optional">
                                        <xs:annotation>
                                          <xs:documentation>Rate at which the amount indexes each policy anniversary.  This value is only applicable when the withdrawal type     is a        monatory amount.</xs:documentation>
                                        </xs:annotation>
                                      </xs:attribute>
                                      <xs:attribute name="charge" type="xs:double" use="optional">
                                        <xs:annotation>
                                          <xs:documentation>Monatory amount of charge for each withdrawal, if applicable. </xs:documentation>
                                        </xs:annotation>
                                      </xs:attribute>
                                      <xs:attribute name="nextWithdrawalDate" type="xs:dateTime" use="optional"/>
                                      <xs:attribute name="finalWithdrawalDate" type="xs:dateTime" use="optional"/>
                                      <xs:attribute name="isGrossAmount" type="xs:boolean" use="required"/>
                                      <xs:attribute name="fundSplitType" type="enums:EnumFundSplitType" use="optional"/>
                                    </xs:complexType>
                                  </xs:element>
                                </xs:sequence>
                                <xs:attribute name="incomeStartDate" type="xs:dateTime" use="required"/>
                              </xs:complexType>
                            </xs:element>
                            <xs:element maxOccurs="unbounded" minOccurs="0" name="ULRiskBenefitChange">
                              <xs:annotation>
                                <xs:documentation>Allows for changes to UL risk benefits in the future.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                <xs:all>
                                  <xs:element name="RevisedULBenefit" type="generaltypes:RiskBenefit"/>
                                </xs:all>
                                <xs:attribute name="dateOfChange" type="xs:dateTime" use="required"/>
                              </xs:complexType>
                            </xs:element>
                            <xs:element minOccurs="0" name="FundSwitch">
                              <xs:annotation>
                                <xs:documentation>Details of the fund switch which is to occur before the quote starts</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element maxOccurs="unbounded" name="SwitchRow">
                                    <xs:complexType>
                                      <xs:attribute name="fromFund" type="xs:string" use="required"/>
                                      <xs:attribute name="toFund" type="xs:string" use="required"/>
                                      <xs:attribute name="switchPercentage" type="xs:double" use="required"/>
                                    </xs:complexType>
                                  </xs:element>
                                  <xs:element name="ApplySwitchFee" type="xs:boolean"/>
                                </xs:sequence>
                              </xs:complexType>
                            </xs:element>
                            <xs:element minOccurs="0" name="AmmendedLifeInfo" type="generaltypes:RiskAssessmentInfo">
                              <xs:annotation>
                                <xs:documentation>If the risk assessment information on this policy has been changed, the update information is captured here. </xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element default="false" minOccurs="0" name="AutomaticFundSwitch" type="xs:boolean">
                              <xs:annotation>
                                <xs:documentation>Determines whether or not the policy is to be automatically switched to less risky funds closer to the policy maturing.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element minOccurs="0" name="AmendPolicyDebt" type="xs:double">
                              <xs:annotation>
                                <xs:documentation>If set this value will be used as the policy debt (a positive value indicates a positive debt), i.e. this will replace the value for total debt on the policy data</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                          </xs:sequence>
                        </xs:complexType>
                      </xs:element>
                      <xs:element minOccurs="0" name="TargetingInfo">
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element name="TargetMethod">
                              <xs:complexType>
                                <xs:choice>
                                  <xs:element name="TargetFund" type="xs:double"/>
                                  <xs:element name="MaximumTaxFreeAmount" type="xs:boolean"/>
                                  <xs:element name="PercentageOfSalAtRetirement" type="xs:double"/>
                                  <xs:element name="PensionAtRetirement" type="xs:double"/>
                                  <xs:element name="PensionInTodaysTerms" type="xs:double"/>
                                </xs:choice>
                                <xs:attribute name="achieveTargetOnDate" type="xs:dateTime" use="required"/>
                                <xs:attribute name="reviewFutureDate" type="xs:dateTime" use="required"/>
                              </xs:complexType>
                            </xs:element>
                          </xs:sequence>
                        </xs:complexType>
                      </xs:element>
                    </xs:all>
                  </xs:complexType>
                </xs:element>
                <xs:element name="NonUnitLinked">
                  <xs:complexType>
                    <xs:all>
                      <xs:element minOccurs="0" name="Developments">
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element minOccurs="0" name="PolicyEndDate" type="xs:dateTime">
                              <xs:annotation>
                                <xs:documentation>Is supplied, this value will override the policy end date contained in the policy data</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element minOccurs="0" name="PremiumInfoChange">
                              <xs:annotation>
                                <xs:documentation>Allows for changes in premium info.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                <xs:attribute name="method" type="enums:EnumPaymentMode" use="optional"/>
                                <xs:attribute name="frequency" type="enums:EnumFrequency" use="optional"/>
                              </xs:complexType>
                            </xs:element>
                            <xs:element maxOccurs="unbounded" minOccurs="0" name="NULRiskBenefitChange">
                              <xs:annotation>
                                <xs:documentation>Allows for changes to NUL risk benefits in the future.</xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                <xs:sequence>
                                  <xs:element maxOccurs="unbounded" name="RevisedNULBenefit" type="generaltypes:RiskBenefit"/>
                                </xs:sequence>
                                <xs:attribute name="dateOfChange" type="xs:dateTime" use="required"/>
                                <xs:attribute name="isIndexing" type="xs:boolean" use="required"/>
                                <xs:attribute name="changed" type="xs:boolean" use="required"/>
                              </xs:complexType>
                            </xs:element>
                            <xs:element minOccurs="0" name="AmmendedLifeInfo" type="generaltypes:RiskAssessmentInfo">
                              <xs:annotation>
                                <xs:documentation>If the risk assessment information on this policy has been changed, the update information is captured here. </xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element minOccurs="0" name="ConversionOption" type="xs:boolean"/>
                            <xs:element minOccurs="0" name="IncludeGIO" type="xs:boolean"/>
                            <xs:element minOccurs="0" name="IncomeConfirmed" type="xs:boolean"/>
                          </xs:sequence>
                        </xs:complexType>
                      </xs:element>
                      <xs:element minOccurs="0" name="MortgageInterestRate" type="xs:double">
                        <xs:annotation>
                          <xs:documentation>Allows the user to supply the mortgage interest rate (for mortgage type policies). </xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element minOccurs="0" name="AdditionalIPInfo">
                        <xs:annotation>
                          <xs:documentation>Additional information for Income Protection cases</xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:attribute name="directIPCoverAmount" type="xs:double" use="required"/>
                          <xs:attribute name="pensionContributionCover" type="xs:double" use="required"/>
                        </xs:complexType>
                      </xs:element>
                    </xs:all>
                  </xs:complexType>
                </xs:element>
              </xs:choice>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="PensionInformation">
            <xs:annotation>
              <xs:documentation>Can be set for pensions. Contains information such as salary, tax rates, etc</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:all>
                <xs:element minOccurs="0" name="fundType" type="xs:string">
                  <xs:annotation>
                    <xs:documentation>Type of the fund for PRSA.</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element name="Salary">
                  <xs:complexType>
                    <xs:attribute name="annualSalary" type="xs:double" use="required"/>
                    <xs:attribute name="salaryEscalation" type="xs:double" use="optional"/>
                    <xs:attribute name="effectiveDate" type="xs:dateTime" use="optional"/>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="TaxRates">
                  <xs:complexType>
                    <xs:attribute name="personalTaxRate" type="xs:double" use="required"/>
                    <xs:attribute name="corporateTaxRate" type="xs:double" use="required"/>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="PensionGuaranteePeriod" type="xs:int"/>
                <xs:element minOccurs="0" name="PensionEscalationRate" type="xs:double"/>
                <xs:element minOccurs="0" name="SpouseInfo">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element minOccurs="0" name="SpousesPensionPercentage" type="xs:double"/>
                      <xs:element minOccurs="0" name="Overlap" type="xs:boolean"/>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="RetainedBenefits" type="xs:double">
                  <xs:annotation>
                    <xs:documentation>Any expected amount of pension funding external to this policy that will be available to the client at retirement. </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="ServiceTerm">
                  <xs:annotation>
                    <xs:documentation>Records the term of service (this is required part of the quote for some products). </xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:choice>
                      <xs:element name="YearsOfServiceAtRetirement" type="xs:int"/>
                      <xs:element name="ServiceDates">
                        <xs:complexType>
                          <xs:attribute name="dateJoinedService" type="xs:dateTime" use="required"/>
                          <xs:attribute name="dateLeftService" type="xs:dateTime" use="required"/>
                        </xs:complexType>
                      </xs:element>
                    </xs:choice>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="ExecPensionInfo">
                  <xs:annotation>
                    <xs:documentation>Additional information required on executive pensions</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:all>
                      <xs:element name="DateJoinedService" type="xs:dateTime"/>
                      <xs:element minOccurs="0" name="DateLeftService" type="xs:dateTime"/>
                      <xs:element minOccurs="0" name="Directorship" type="enums:EnumDirectorship"/>
                      <xs:element minOccurs="0" name="MaxLumpSumEntitlement" type="xs:double"/>
                      <xs:element minOccurs="0" name="serviceToDate" type="xs:int"/>
                    </xs:all>
                  </xs:complexType>
                </xs:element>
              </xs:all>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="PriceMatch">
            <xs:annotation>
              <xs:documentation>Supplied if there is a price match to be performed as part of the quote.</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:all>
                <xs:element name="PriceToMatch" type="xs:double"/>
              </xs:all>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="TestMode" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>This can be specified on a policy to say that this case in a test case .  This will force the engine to produce more output values which help in the testing purposes.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="ValidationOverride">
            <xs:complexType>
              <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" name="OverrideMinRow">
                  <xs:complexType>
                    <xs:attribute name="parameterName" type="enums:EnumParameterName" use="required"/>
                    <xs:attribute name="newMinValue" type="xs:double" use="required"/>
                  </xs:complexType>
                </xs:element>
                <xs:element maxOccurs="unbounded" minOccurs="0" name="OverrideMaxRow">
                  <xs:complexType>
                    <xs:attribute name="parameterName" type="enums:EnumParameterName" use="required"/>
                    <xs:attribute name="newMaxValue" type="xs:double" use="required"/>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="BypassValidation" type="xs:boolean"/>
                <xs:element minOccurs="0" name="Bypass7030Rule" type="xs:boolean"/>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="PointOfIssue">
            <xs:complexType>
              <xs:all>
                <xs:element name="RegularPremium" type="xs:double"/>
              </xs:all>
            </xs:complexType>
          </xs:element>
        </xs:all>
      </xs:complexType>
    </xs:schema>
    <xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://ulquoteoutput.framework.domain.projections.bil.ie" xmlns:enums="http://enums.common.framework.domain.projections.bil.ie" xmlns:generaltypes="http://generaltypes.common.framework.domain.projections.bil.ie" attributeFormDefault="unqualified" elementFormDefault="qualified" targetNamespace="http://ulquoteoutput.framework.domain.projections.bil.ie">
      <xs:import namespace="http://enums.common.framework.domain.projections.bil.ie"/>
      <xs:import namespace="http://generaltypes.common.framework.domain.projections.bil.ie"/>
      <xs:element name="ULQuoteOutputRoot" type="ULQuoteOutput"/>
      <xs:complexType name="ULQuoteOutput">
        <xs:annotation>
          <xs:documentation>Data that defines the Unit Linked Quotation Output</xs:documentation>
        </xs:annotation>
        <xs:sequence>
          <xs:element name="ClientAssumptions">
            <xs:complexType>
              <xs:sequence>
                <xs:element maxOccurs="unbounded" name="LifeDetailsRow">
                  <xs:complexType>
                    <xs:all>
                      <xs:element name="AssumedAgeYears" type="xs:int">
                        <xs:annotation>
                          <xs:documentation>The clients age in whole years used in the calculations.</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element name="AssumedAgeMonths" type="generaltypes:NrOfMonthsRange">
                        <xs:annotation>
                          <xs:documentation>The clients months in addition to age in years used in the calculations.</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element name="AgeNextBirthday" type="xs:string">
                        <xs:annotation>
                          <xs:documentation>The clients age next birthday used when looking up benefit rates.</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                    </xs:all>
                    <xs:attribute name="forLife" type="enums:EnumLife" use="required">
                      <xs:annotation>
                        <xs:documentation>FIX: Add life enum</xs:documentation>
                      </xs:annotation>
                    </xs:attribute>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="NormalRetirementAge">
                  <xs:annotation>
                    <xs:documentation>If this is a Pension Projection, this field holds the client expected retirement age in years and months</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:attribute name="years" type="xs:int" use="required"/>
                    <xs:attribute name="months" type="xs:int" use="required"/>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element maxOccurs="unbounded" name="BasisRow">
            <xs:complexType>
              <xs:all>
                <xs:element name="Disclosure" type="DisclosureInfo">
                  <xs:annotation>
                    <xs:documentation>This is the disclosure information that will appear on any disclsoure letter. </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="ChangeInDisclosure" type="DisclosureInfo">
                  <xs:annotation>
                    <xs:documentation>If there has been a topup on the policy (i.e. the Disclosure node above contains the difference table) then this node contains the full post change disclosure information. </xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="TestOutput">
                  <xs:annotation>
                    <xs:documentation>This output is produced when the system is running in test mode.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" name="MonthRow">
                        <xs:annotation>
                          <xs:documentation>This is the Pre Change monthly output values used for testing the engine. </xs:documentation>
                        </xs:annotation>
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element maxOccurs="unbounded" minOccurs="0" name="BenefitCharges">
                              <xs:annotation>
                                <xs:documentation>Contains the monthly charge  for each benefit. </xs:documentation>
                              </xs:annotation>
                              <xs:complexType>
                                <xs:attribute name="benefitId" type="enums:EnumRiskBenefit" use="required"/>
                                <xs:attribute name="chargeThisMth" type="xs:double" use="required"/>
                              </xs:complexType>
                            </xs:element>
                          </xs:sequence>
                          <xs:attribute name="month" type="xs:int" use="required"/>
                          <xs:attribute name="rpPaid" type="xs:double" use="required"/>
                          <xs:attribute name="rpAllocated" type="xs:double" use="required"/>
                          <xs:attribute name="spAllocated" type="xs:double" use="required"/>
                          <xs:attribute name="fundBeforeBenChg" type="xs:double" use="required"/>
                          <xs:attribute name="fundsAfterBenChg" type="xs:double" use="required"/>
                          <xs:attribute name="fundBeforeFRCSP" type="xs:double" use="required"/>
                          <xs:attribute name="fundBeforeGrowth" type="xs:double" use="required"/>
                          <xs:attribute name="growth" type="xs:double" use="required"/>
                          <xs:attribute name="totalComm" type="xs:double" use="required"/>
                          <xs:attribute name="bonus" type="xs:double" use="required"/>
                          <xs:attribute name="fundCommission" type="xs:double" use="required"/>
                          <xs:attribute name="income" type="xs:double" use="required"/>
                          <xs:attribute name="taxFreePremium" type="xs:double" use="required"/>
                          <xs:attribute name="surrenderBeforeTax" type="xs:double" use="required"/>
                          <xs:attribute name="surrenderAfterTax" type="xs:double" use="required"/>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="LoyaltyBonus">
                  <xs:complexType>
                    <xs:attribute name="totalBonus" type="generaltypes:FormattedDouble" use="required"/>
                    <xs:attribute name="terminalBonus" type="generaltypes:FormattedDouble" use="optional"/>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="PartSurrenderInformation">
                  <xs:annotation>
                    <xs:documentation>Information about part surrendrs to be paid from the policy. </xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element maxOccurs="unbounded" minOccurs="0" name="PartSurrenderRow">
