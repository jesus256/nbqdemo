                                <xs:attribute name="perMilleTermYears" type="xs:int" use="optional"/>
                                <xs:attribute name="perMilleCost" type="xs:double" use="optional"/>
                                <xs:attribute name="percentRating" type="xs:double" use="optional"/>
                                <xs:attribute name="percentCost" type="xs:double" use="optional"/>
                              </xs:complexType>
