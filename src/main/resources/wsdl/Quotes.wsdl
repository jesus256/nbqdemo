                          <xs:attribute name="averageCost" type="xs:double" use="required"/>
                          <xs:attribute name="benefitAmount" type="xs:double" use="optional"/>
                          <xs:attribute name="benefitTermYears" type="xs:int" use="optional"/>
                          <xs:attribute name="forLife" type="enums:EnumLife"/>
