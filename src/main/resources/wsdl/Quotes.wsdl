              <xs:choice>
                <xs:element name="UnitLinked">
                  <xs:complexType>
                    <xs:all>
                      <xs:element name="QuoteType" type="enums:EnumQuoteType">
                        <xs:annotation>
                          <xs:documentation>Describes the type of quote to be performed (e.g. ESV, target pension etc)</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element name="QuotationBasedOn" type="xs:int">
                        <xs:annotation>
                          <xs:documentation>Specifies what quotation is based on - UI input number.</xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element name="ScenarioBasis">
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element minOccurs="0" name="QuoteEndDate" type="xs:dateTime">
                              <xs:annotation>
                                <xs:documentation>Date of projection end. Any projection beyond policy end dste will assume the policy id PUPed. If not supplied then the quote will run until the policy end date.</xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="unbounded" minOccurs="0" name="BasisRow" type="generaltypes:BasisRowDef">
                              <xs:annotation>
                                <xs:documentation>If no basis rows are supplied the engine will use the default basis rows defined in the projections ref for the product categroy being quoted. </xs:documentation>
                              </xs:annotation>
                            </xs:element>
                            <xs:element maxOccurs="unbounded" minOccurs="0" name="NotSelectedBasisRow" type="generaltypes:BasisRowDef">
                              <xs:annotation>
                                <xs:documentation>Not selected basis rows. Stored for retrieval purposes. </xs:documentation>
                              </xs:annotation>
                            </xs:element>
                          </xs:sequence>
                        </xs:complexType>
                      </xs:element>
                      <xs:element minOccurs="0" name="Developments">
                        <xs:complexType>
                          <xs:sequence>
                            <xs:element minOccurs="0" name="PolicyEndDate" type="xs:dateTime">
                              <xs:annotation>
                                <xs:documentation>Is supplied, this value will override the policy end date contained in the policy data</xs:documentation>
                              </xs:annotation>
                            </xs:element>
