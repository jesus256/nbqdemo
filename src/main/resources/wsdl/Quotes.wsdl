                    <xs:documentation>For tacking edit consultations, this is the database reference id for scource consultation</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="reportGenerated" type="xs:boolean"/>
                <xs:element name="UserId" type="xs:string">
