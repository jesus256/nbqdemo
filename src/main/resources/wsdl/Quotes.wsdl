                </xs:element>
                <xs:element minOccurs="0" name="DateOfJoiningService" type="xs:dateTime">
                  <xs:annotation>
                    <xs:documentation>If known this is the date that the client joined their current employer (only used for purpose of EBQ user interface)</xs:documentation>
                  </xs:annotation>
                </xs:element>
                <xs:element minOccurs="0" name="LastKnownSalary">
                  <xs:annotation>
                    <xs:documentation>If known this was the clients salary (only used for purpose of EBQ user interface).</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:attribute name="annualSalary" type="xs:double" use="required"/>
