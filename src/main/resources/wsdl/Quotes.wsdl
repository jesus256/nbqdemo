                          <xs:attribute name="OSurrenderDate" type="xs:date" use="required"/>
                          <xs:attribute name="OPartSurrAmtBeforeTax" type="generaltypes:FormattedDouble" use="required"/>
                          <xs:attribute name="OPartSurrAmtAfterTax" type="generaltypes:FormattedDouble" use="required"/>
                          <xs:attribute name="OPartSurrTaxAmt" type="generaltypes:FormattedDouble" use="required"/>
                          <xs:attribute name="OPartSurrPenalty" type="generaltypes:FormattedDouble" use="required"/>
                          <xs:attribute name="OPartSurrFee" type="generaltypes:FormattedDouble" use="required"/>
                        </xs:complexType>
                      </xs:element>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="SavingsSpecificDetails">
                  <xs:annotation>
                    <xs:documentation>Information specific to savings policies only.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:all>
                      <xs:element minOccurs="0" name="GrossIncomeAmountPA" type="generaltypes:FormattedDouble">
                        <xs:annotation>
                          <xs:documentation>This is the summation of 12months of income from the first income payment. </xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element minOccurs="0" name="SumAssured" type="generaltypes:FormattedDouble">
                        <xs:annotation>
                          <xs:documentation>The sum assured for LifeCover for life 1. </xs:documentation>
                        </xs:annotation>
                      </xs:element>
                      <xs:element minOccurs="0" name="SustainableTerm" type="xs:int"/>
                      <xs:element minOccurs="0" name="FinalIncomeAmt" type="generaltypes:FormattedDouble"/>
                      <xs:element minOccurs="0" name="LastIncomeDate" type="xs:date"/>
                      <xs:element minOccurs="0" name="NetIncomeAmountPA"/>
                      <xs:element minOccurs="0" name="FullIncomeReducedDate" type="xs:date"/>
                      <xs:element minOccurs="0" name="FullIncomeReducedDisclosureYear" type="xs:int"/>
                    </xs:all>
                  </xs:complexType>
                </xs:element>
                <xs:element minOccurs="0" name="MortgageSpecificDetails">
                  <xs:annotation>
                    <xs:documentation>Information applicable to mortgage policies only.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                    <xs:all>
                      <xs:element minOccurs="0" name="EndowmentInterestPayment" type="generaltypes:FormattedDouble"/>
                      <xs:element minOccurs="0" name="EndowmentTotal" type="generaltypes:FormattedDouble"/>
                      <xs:element minOccurs="0" name="EndowmentAverageTaxRelief" type="generaltypes:FormattedDouble"/>
                      <xs:element minOccurs="0" name="EndowmentAverageNetOutlay" type="generaltypes:FormattedDouble"/>
                      <xs:element minOccurs="0" name="RepaymentMortgageProtection" type="generaltypes:FormattedDouble"/>
                      <xs:element minOccurs="0" name="RepaymentPaymentToLender" type="generaltypes:FormattedDouble"/>
